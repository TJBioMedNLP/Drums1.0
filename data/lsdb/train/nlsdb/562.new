    <!--

        var idCourant1;
        var idCourant2;
        var etatInitial;

        function initEnRelief() {
            if (!etatInitial) {
                // Etat de départ de la mise en premier plan
                idCourant1 = null;
                idCourant2 = null;
                for (id=1; id<=8; id++) {
                    initTransparence('acces' + id);
                    cache('aide' + id);
                }
                // Image centrale
                initTransparence('cercle');
                // Etat
                etatInitial=true;
            }
        }
        function premierPlan(id1,id2) {
            // On met en relief le lien selectionne et la div associée
            if (idCourant1 != id1) {
                idCourant1 = id1;
                idCourant2 = id2;
                objetCourant1 = document.getElementById(id1);
                objetCourant2 = document.getElementById(id2);
                // Effacement image centrale
                transparent('cercle');
                // Lien acces non transparent
                objetCourant1.style.filter='alpha(opacity=100)';
                objetCourant1.style.opacity='1';
                // Div centrale visible
                objetCourant2.style.display='block';
                // On cache les autres div
                for (id=1; id<=8; id++) {
                    if (idCourant1 != 'acces' + id) {
                       transparent('acces' + id);
                       cache('aide' + id);
                    }
                }
                // Etat
                etatInitial=false;
            }
        }
        function initTransparence(id) {
            var d = document.getElementById(id);
            if (d) {
              d.style.filter='alpha(opacity=100)';
              d.style.opacity='1';
              if (id != 'cercle') d.style.background='';
            }
        }
        function transparent(id) {
            var d = document.getElementById(id);
            if (d) {
              d.style.filter='alpha(opacity=25)';
              d.style.opacity='.25';
              if (id != 'cercle') d.style.background='';
            }
        }
        function cache(id) {
            var d = document.getElementById(id);
            if (d) {
              d.style.display='none';
            }
        }
        function montreCache(id) {
            // On cache toutes les autres div contextuelles
            if (id != 'contextuelle5') cache('contextuelle5');
            // On affiche id si elle est cachee
            var d = document.getElementById(id);
            if (d) {
              if (d.style.display == '' || d.style.display == 'none') {
                d.style.display='block';
              } else {
                d.style.display='none';
              }
            }
        }

    //-->
	</script>

