<Script language="JavaScript">
function check_submit(objForm)
{
	var pat1 = /-?\d+.?\d?/;
	
	if(objForm.bit_score.checked == false && (objForm.score.value == "" || objForm.score.value == null) )
	{
		alert("If you do not select the \"by default\" checkbox,\nplease input a bit score!!!");
		event.returnValue = false;
	}
	else if(!pat1.test(objForm.score.value) && objForm.bit_score.checked == false)
	{
		alert("Please input a bit score, e.g. -5 !");
		event.returnValue = false;
	}
	
}
</Script>

