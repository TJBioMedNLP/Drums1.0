<script language="JavaScript">
function chkall(input1,input2)
{
    var objForm = document.forms[input1];
    var objLen = objForm.length;
    for (var iCount = 0; iCount < objLen; iCount++)
    {
        if (input2.checked == true)
        {
            if ((input2.value == "serine") && (objForm.elements[iCount].type == "checkbox") && (objForm.elements[iCount].name == "S_KINASE_COM[]"))
		       objForm.elements[iCount].checked = true;
            if ((input2.value == "threonine") && (objForm.elements[iCount].type == "checkbox") && (objForm.elements[iCount].name == "T_KINASE_COM[]"))
		       objForm.elements[iCount].checked = true;
			if ((input2.value == "tyrosine") && (objForm.elements[iCount].type == "checkbox") && (objForm.elements[iCount].name == "Y_KINASE_COM[]"))
		       objForm.elements[iCount].checked = true;
            if ((input2.value == "histidine") && (objForm.elements[iCount].type == "checkbox") && (objForm.elements[iCount].name == "H_KINASE_COM[]"))
		       objForm.elements[iCount].checked = true;		
        }     
        else
        {
            if ((input2.value == "serine") && (objForm.elements[iCount].type == "checkbox") && (objForm.elements[iCount].name == "S_KINASE_COM[]"))
		       objForm.elements[iCount].checked = false;
            if ((input2.value == "threonine") && (objForm.elements[iCount].type == "checkbox") && (objForm.elements[iCount].name == "T_KINASE_COM[]"))
		       objForm.elements[iCount].checked = false;
			if ((input2.value == "tyrosine") && (objForm.elements[iCount].type == "checkbox") && (objForm.elements[iCount].name == "Y_KINASE_COM[]"))
		       objForm.elements[iCount].checked = false;
            if ((input2.value == "histidine") && (objForm.elements[iCount].type == "checkbox") && (objForm.elements[iCount].name == "H_KINASE_COM[]"))
		       objForm.elements[iCount].checked = false;			
        }
    }
}
</script>

