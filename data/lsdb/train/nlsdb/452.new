	el = document.getElementById("alignment_input");
	el.value= "A  [13 13  3  1 54  1  1  1  0  3  2  5 ]\nC  [13 39  5 53  0  1 50  1  0 37  0 17 ]\nG  [17  2 37  0  0 52  3  0 53  8 37 12 ]\nT  [11  0  9  0  0  0  0 52  1  6 15 20 ] ";
}


