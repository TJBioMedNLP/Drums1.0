		<script type="text/javascript">

		function findFirstRuleInSheet(sheet, name) {
			var rules = sheet.cssRules;
			for (var ruleno=0 ; ruleno<rules.length ; ++ruleno) {
				var rule = rules[ruleno].cssText;
				var ruleparts = rule.split(/[{}]/);
				var rulenames = rule.split(" ");
				for (var i=0 ; i<rulenames.length ; ++i)
					if (name==rulenames[i]) break;
				if (i<rulenames.length) break;
			}
			return ruleno==rules.length ? null : rules[ruleno];
		}

		function findFirstRule(name) {
			var stylesheets = document.styleSheets;
			for (var sheetno=0 ; sheetno<stylesheets.length ; ++sheetno) {
				if (style=findFirstRuleInSheet(stylesheets[sheetno],name)) break;
			}
			return style;
		}

		function getElementsOfClass(parentnode, name) {
			var nodes = new Array();
			var children = parentnode.childNodes;
			for (var i=0 ; i<children.length ; ++i) {
				nodes = nodes.concat(getElementsOfClass(children[i], name));
			}
			if (parentnode.className==name) {
				nodes = nodes.concat(parentnode);
			}
			return nodes;
		}
		
