<script language="javascript">

var scrollerdelay='3000' 
var scrollerwidth='240px'
var scrollerheight='50px'
var scrollerbgcolor='#ffffff'
var scrollerbackground=''
//CONTENUTI
var messages=new Array()

messages[0]="<font face='Arial'><a href='/cont/3610arc/0509/0601/'>EULAR's First Course on Advanced Rheumatological Ultrasound</a></font>"

