					<TD width="350px">
						<BR>
						<b>For proteins with attributes : </b>
					</TD>
					<TD>&nbsp;&nbsp;</TD>
				</TR>
				<TR>
					<TD width="350px">N-terminal Export Signal</TD>
					<TD>
						<input type=radio name=pro_signal value=Y> Yes 
						<input type=radio name=pro_signal value=N> No 
						<input type=radio name=pro_signal value=B checked="checked"> don't care
					</TD>
				</TR>
				<TR>
					<TD width="350px">GPI anchor</TD>
					<TD>
						<input type=radio name="gpianchor" value="Y"> Yes 
						<input type=radio name="gpianchor" value="N"> No 
						<input type=radio name="gpianchor" value=B checked="checked"> don't care
					</TD>
				</TR>
				<TR>
					<TD width="350px">Transmembrane domains</TD>
					<TD>
						<input type=radio name=transmembrane value=Y> Yes 
						<input type=radio name=transmembrane value=N> No 
						<input type=radio name=transmembrane value=B checked="checked"> don't care
					</TD>
				</TR>
				<TR>
					<TD width="350px">Keyword from description (optional) : </TD>
					<TD width="350px">
						<INPUT type="text" size="60" maxlength="30" name="searchfor" value="">
					</TD>
				</TR>
				<TR>
					<TD width="350px">
						<BR>
						<b>that contain repeats : </b>
					</TD>
					<TD>&nbsp;&nbsp;</TD>
				</TR>
				<TR>
					<TD width="350px">P-value below</TD>
					<TD>10e
						<input type=text name="pe" value="-5" size="4" maxlength="4">
					</TD>
				</TR>
				<TR>
					<TD width="350px">N-glycosylation</TD>
					<TD>
						<input type=radio name=glycosylated value=Y> Yes 
						<input type=radio name=glycosylated value=N> No 
						<input type=radio name=glycosylated value=B checked="checked"> don't care
					</TD>
				</TR>
