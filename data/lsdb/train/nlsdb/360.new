function setStatusLine(str) {
	window.status = str;
	return true;
}
function setFullname(fn) {
    window.status = fn;
    this.document.species.fullname.value = fn;
}
function disp_sp_list(event, key, sta) {
    var doc = document;

    var elm = doc.getElementById(key);
    var x, y;
    if (document.all) {
        x = window.event.clientX + document.body.scrollLeft + document.documentElement.scrollLeft;
        y = window.event.clientY + document.body.scrollTop + document.documentElement.scrollTop;

        //
        var d = 2;
        if ((event.type == 'mouseout') &&
            (elm.offsetLeft < x - d) && (x < elm.offsetLeft + elm.offsetWidth) &&
            (elm.offsetTop < y - d) && (y < elm.offsetTop + elm.offsetHeight)) {
            return;
        }
    }
    else {
        x = event.pageX;
        y = event.pageY;
        if ((event.type == 'mouseout') &&
            (elm.offsetLeft < x) && (x < elm.offsetLeft + elm.offsetWidth) &&
            (elm.offsetTop < y) && (y < elm.offsetTop + elm.offsetHeight)) {
            return;
        }
    }

    elm.style.display = 'inline';
    if (!sta) {
        elm.style.visibility = 'hidden';
    }

    if (document.all) {
        elm.style.left       = window.event.clientX - 10 + 'px';
    }
    else {
        elm.style.left       = event.pageX - 10 + 'px';
    }

    if (document.all) {
//        elm.style.top = window.event.clientY + doc.body.scrollTop -10 + "px";
        elm.style.top = window.event.clientY + doc.documentElement.scrollTop -10 + "px";
    }
    else {
        elm.style.top        = event.pageY - 10 + 'px';
    }

    if (sta) {
        elm.style.visibility = 'visible';
    }

    return;
}
function disp_speclist(superkingdom, phylum) {

    var url = '/htbin/disp_speclist'
            + '?' + 'superkingdom=' + superkingdom
            + '&' + 'phylum=' + phylum;
    open(url, 'disp_speclist');

    return;
}
function open_taxwin(key) {
    var args = '';
    var tax = key.split('-');
    var n = tax.length;
    for(var i = 1; i < n; i++) {
        var rank = tax[i].split('.');

        if (args != '') {
            args += '&';
        }
        args += rank[0] + '=' + rank[1];
    }
    var url = '/htbin/species.cgi' + '?' + args;
    var win = open(url, 'taxwin');
    win.focus();

    var elm = document.getElementById(key);
    elm.style.visibility = 'hidden';    

    return;
}
//-->
