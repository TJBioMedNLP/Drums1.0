ic=49297;
im=233280;
jran = (jran*ia+ic) % im;
var whichImage = Math.ceil( (jran/(im*1.0)) * anzahl);
function showImage(){
document.write('<img src="/Images/HeaderPics/'+whichImage+'.jpg" alt="'+whichImage+'">');
}
 function staticLoadScript()
 {
 document.write('<script src="/Images/HeaderPics/',whichImage,'.js"type="text/javascript"><\/script>');
 }

</script>

