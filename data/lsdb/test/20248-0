			if( document.cookie == '' )
			{	var track = 'true'; // if no cookies set, default to true
			}else
			{
				var matches = document.cookie.match( /(;|^) track=(true|false);/ );
	
				if( matches == null ) // nothing has been set, to track is the default
					var track = 'true';
				else
					var track = matches[2]; // will be either (string) 'true' or 'false'
	
				track = (track == 'true'); //convert string to bool
			}

		//	ok, now track (or not)
			if( track )   // track is a string, not 
			{	//alert( 'tracking' );
				var pageTracker = _gat._getTracker("UA-1725473-1");
                                pageTracker._trackPageview();
			}/*else //else just do nothing
			{	alert( "not tracking" );
			}*/
