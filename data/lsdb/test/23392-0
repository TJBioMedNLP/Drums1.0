	function toggleAll1() {
		fo1 = document.getElementById("switch1");

		foo1 =document.getElementById("f1");

		if (fo1.innerHTML=='Collapse')
		{
			fo1.innerHTML='Expand All';
			 foo1.className='firstfive';
		}
		else
		{
			fo1.innerHTML='Collapse';
				foo1.className='';
		}

		return;
	}


