<Script Language="JavaScript">

function MenuOver(mobj){
    mobj.style.backgroundColor="#A4A4A4";
    mobj.style.color="#FFFFFF";
}

function MenuOut(mobj){
    mobj.style.backgroundColor="#efefef";
    mobj.style.color="#A4A4A4";
}

function MenuClick(URL){
    this.location.href(URL);
}


function MenuOver2(mobj){
	//mobj.style.fontSize="14pt";
    mobj.style.fontStyle="Italic";
}

function MenuOut2(mobj){
	//mobj.style.fontSize="12pt";
    mobj.style.fontStyle="normal";
}




</Script>


