<h2 align=center>Welcome to COUDES ! A web server for beta-turn prediction.</h2>

<script type="text/javascript">
if (document.images)
{
    image1 = new Image;
    image2 = new Image;
    image1.src = "enter1.jpg";
    image2.src = "enter2.jpg";
}

function chgImg(name, image)
{
    if (document.images)
    {
        document[name].src = eval(image+".src");
    }
}
</script>

<p align=center><a href="COUDES.html" onMouseOver='chgImg("enter", "image2")'
onMouseOut='chgImg("enter", "image1")'><img name="enter"
src="enter1.jpg" border="0"></a>
