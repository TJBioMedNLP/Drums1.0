<script type="text/javascript">


 
 function toggleAbstractDisplay(publication){

    if(document.getElementById(publication).style.display=='block')

    {

        document.getElementById(publication).style.display = 'none'; 

    }

    else 

    {

        document.getElementById(publication).style.display = 'block';

    }

 }
 
 function printVersion(activeButton){
    newwindow=window.open("","","toolbar=no,status=no,width=610,height=400,location=no,scrollbars=yes,menubar=yes,resizable=yes");
    newdocument=newwindow.document;
    newdocument.write('<html><head><link type="text/css" href="/css/sitecore.css" rel="StyleSheet" /><link type="text/css" href="/styles/printFaculty.css" rel="StyleSheet" /></head><body bgcolor="#FFFFFF"><img class="logo" src="/images/display/logo.gif" alt="Kellogg School of Management"><br />');
    if (document.getElementById(activeButton))
    {
    
        newdocument.write('<div id="right">');
        newdocument.write(document.getElementById('right').innerHTML);
        newdocument.write('</div>');
        newdocument.write('<div id="topSection">');
        newdocument.write(document.getElementById('topSection').innerHTML);
        newdocument.write('</div>');
        newdocument.write('<div style="clear:left"></div>');
        if(activeButton == 'leftTeaching')
        {
            newdocument.write(document.getElementById(activeButton).innerHTML);
            newdocument.write(document.getElementById(activeButton+'2').innerHTML);
        }
        else
        {
            newdocument.write(document.getElementById(activeButton).innerHTML);
        }

    }
    else 
    {
        newdocument.write("There is a problem with the print version of this page.&nbsp;&nbsp;Please contact the Web Team")
    }
    newdocument.write("<div class='footer'>"+document.location.href+"</div></body></html>");
    newdocument.close();
}

function switchHomeTabs (activeButton) {
	var homeTabsIDs = new Array('biography', 'vita', 'research', 'teaching');
	for(i = 0; i < homeTabsIDs.length; i++)
	{
 		document.getElementById(homeTabsIDs[i]+"Print").style.display = 'none';

	}
    document.getElementById(activeButton+"Print").style.display = 'block';    
}


</script>
