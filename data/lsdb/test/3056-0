function imageon(name)   {
        document[name].src = eval(name + "on.src");
}


function imageoff(name)  {
        document[name].src = eval(name + ".src");
}


function on(name)  {
        if (check == true){
           imageon(name);
        }}


function off(name)  {
        if (check == true){
           imageoff(name);
        }}
