		mfElement = function() {};
		mfElement.prototype = {
			setOptions: function(element, options) {
				this.element = $(element);
				this.options = {
      				element:       'default'
				}
    		Object.extend(this.options, options || {});
			}
		}

		mfImage = Class.create();
		mfImage.prototype = Object.extend(new mfElement(), {
			initialize: function(element, options) {
				this.setOptions(element, options);
				this.options.rootDir = options.rootDir;
		    	this.folderDir = $(element + "_folder");
				this.imageDir = $(element + "_dd");
				this.image = $(element + "_img");
				if(this.folderDir){
					if(this.folderDir.options.length != 0){
						this.selectedFolder = this.folderDir.options[this.folderDir.options.selectedIndex].value;
					}else{
						this.selectedFolder = '';
					}
				}
				if(this.imageDir.options.length != 0){
					this.selectedImage = this.imageDir.options[this.imageDir.options.selectedIndex].value;
				}else{
					this.selectedImage = '';
				}
				this.aChangeFolder = this.changeFolder.bindAsEventListener(this);
				this.aShowImage = this.showImage.bindAsEventListener(this);
				
				Event.observe(this.folderDir, 'change', this.aChangeFolder);
				Event.observe(this.imageDir, 'change', this.aShowImage);
				this.showImage();
			},
		  	
			changeFolder: function( e ){
				var el = Event.element( e );
				this.selectedFolder = el.options[el.options.selectedIndex].value;
				this.imageDir.innerHTML = '';
				var newList = this.imageFolderList.find( function(li){
					return (li.folder == this.selectedFolder)
				}.bind(this));
				newList.files.each( function(opt){
						/* strip first slash*/
						var rep = this.selectedFolder.substring(1,this.selectedFolder.length);
						var shortname = opt.replace(rep, '');
						new Insertion.Bottom(this.imageDir, '<option value="' + opt + '">' + shortname + '</option>' );
					}.bind(this));
				this.showImage();
				this.element.value = this.selectedImage;
			},
			
			showImage: function( e ){
				if(e){
					var el = Event.element( e );
				}else{
					var el = this.imageDir;
				}
				if(el.options.length == 0){
					this.image.src = '';
					this.selectedImage = '';
				}else{
					this.selectedImage = el.options[el.options.selectedIndex].value;
					this.image.src = this.options.rootDir + this.selectedImage;
				}
			}	
		});
		
		mfField = Class.create();
		mfField.prototype = Object.extend(new mfElement(), {
			initialize: function(element, options) {
				this.setOptions(element, options);
			}
		});	
	 -->
		</script>


