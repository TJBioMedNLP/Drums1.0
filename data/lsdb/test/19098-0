						<script type="text/javascript">
		function submit_form(){
					return false;
		}
			
		function submitbutton(button){
			if(button=="cancel"){
				document.location = 'index2.php?option=com_fabrik&task=viewTable&amp;cid=' + '';
			}
		}
						
		/* duplicates the groups sub group and places it at the end of the group */
		function duplicateGroup(i, addJs){
			var group = $('group'+i);
			var subgroups = group.getElementsByTagName('div');
			var c = 0;
			for (j = 0; j < subgroups.length; j++) {
				if(subgroups[j].className == 'mf_sub_group'){
					c = c+1;
				}
			}		

			if(group.style.display == 'none'){
				group.style.display = 'block';	
			}else{
				var subgroup = $('subgroup'+i+'_0');
				var clone = subgroup.cloneNode(true);
				clone.id = 'subgroup'+i+'_' + c;
				Element.setStyle(clone, {display:'none'});
				c = c + 1;
				group.appendChild(clone);
				Effect.Appear(clone, 
					{duration:0.5}
				);
				
			}
			eval(addJs);
		}
			
		/* removes the last duplicated subgroup */
		
		function deleteLastClone(i, delJs){
			var element = $('group'+i);
			var subgroups = element.getElementsByTagName('div');
			var lastSubGroup  = "";
			var c = 0;
			for (j = 0; j < subgroups.length; j++) {
				if(subgroups[j].className == 'mf_sub_group'){
					c = c+1;
					lastSubGroup = subgroups[j];
				}
			}
			if(c > 1){ // make sure we dont remove the origional sub group
				c = c -1;
				if(lastSubGroup != ''){
					Effect.Fade(lastSubGroup, 
							{duration:0.5,
								afterFinish: function(){
									Element.remove(lastSubGroup);
									eval(delJs);
							}}
					);
				}	
				
			}else{
				if( c == 1){
					c = c -1;
					Effect.Fade(lastSubGroup, 
							{duration:0.5,
								afterFinish: function(){
									element.style.display = 'none';
									eval(delJs);
							}}
					);					
					
				}
			}
		}
		</script>
		<script type="text/javascript" src="http://www.e-meducation.org/includes/js/mambojavascript.js"></script>
		<script language="javascript" type="text/javascript">
		<!--
		fabrikForm = Class.create();
