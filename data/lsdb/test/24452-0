function viewHelp(argument) {

	document.getElementById(Text[argument]).style.visibility = "visible";
}

function blurHelp(argument) { // Layer ausblenden nach 5 sekunden durch Event onMouseOut
	timeOut = window.setTimeout('blurTxt(Text[argument])',3000);
}

function blurTxt(argument) { // Layer ausblenden
	document.getElementById(Text[argument]).style.visibility = "hidden";
}


