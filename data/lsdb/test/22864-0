<script type="text/javascript" >
   var doChange = true;
   function checkKey(value)
   {
      if (window.event.keyCode == 38 || window.event.keyCode == 40)
         doChange = false;
      else if (window.event.keyCode == 13)
         window.location.href = value;
   }

   function changedBookmark(value)
   {
      if (doChange)
         window.location.href = value;
      else
         doChange = true;
   }
</script>
