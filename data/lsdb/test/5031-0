                        <td width="50%"> <p align="center"><b>
                            <script language="JavaScript1.2">
/*
Neon Lights Text
By JavaScript Kit (http://javascriptkit.com)
Over 400+ free scripts here!
*/
var message;
var neonbasecolor="orange"
var neontextcolor="red"
var flashspeed=50  //in milliseconds

///No need to edit below this line/////

var n=0
var tempref; //cache reference to neonlight array
var colr//=neontextcolor; //color used at the moment

function write_neon(mess)
{
	message=mess;
	if (document.all){
		document.write('<font color="'+neonbasecolor+'">')
		for (m=0;m<message.length;m++)
		document.write('<span id="neonlight">'+message.charAt(m)+'</span>')
		document.write('</font>')
		tempref=document.all.neonlight
	}
	else
		document.write(message)
}

function neon(){
//Change all letters to base color
if (n==0){
	if(colr==neontextcolor)
		{colr=neonbasecolor;}
	else
		{colr=neontextcolor;}
}
//cycle through and change individual letters to neon color
tempref[n].style.color=colr;
if (n<tempref.length-1)
	n++
	else{
		n=0
		clearInterval(flashing)
		setTimeout("beginneon()",500)
		return
	}
}

function beginneon(){
if (document.all)
	flashing=setInterval("neon()",flashspeed)
}
beginneon()
</script>
                            <script language="JavaScript">
					write_neon("Ajouts dans les sections 'Articles.'");
				</script></a>
