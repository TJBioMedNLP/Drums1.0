function cOn(td){
if(document.getElementById||(document.all && !(document.getElementById))){
td.style.backgroundColor="black";
}
}

function cOut(td){
if(document.getElementById||(document.all && !(document.getElementById))){
td.style.backgroundColor="#006600";
}
}
</script>
