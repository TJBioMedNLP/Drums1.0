  <td valign="top">
<div id="divMenu" style="width:34px; height:118px; position:absolute; left:900px; top:100px; z-index:1; visibility:visible;">
<script>
  function moveRightEdge() {
    var yMenuFrom, yMenuTo, yOffset, menu_y;
    menu_y = -120;
    yMenuFrom   = parseInt(divMenu.style.top);
    yMenuTo     = document.body.scrollTop;
    if(yMenuTo<menu_y) yMenuTo = 0;
    else yMenuTo = yMenuTo - menu_y;
    if (yMenuFrom != yMenuTo){
      yOffset = Math.ceil(Math.abs(yMenuTo - yMenuFrom) / 20); //스크롤 이동시 상하 속도
      if (yMenuTo < yMenuFrom) yOffset = -yOffset;
        divMenu.style.top = parseInt (divMenu.style.top, 10) + yOffset;
      }
      setTimeout ("moveRightEdge()", 10);
  }
  moveRightEdge();
</script>
