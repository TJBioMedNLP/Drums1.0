$(document).ready(function() {
  var $items = $('.eventitem');
  
  var $first = $items.filter(':eq(0), :eq(1), :eq(2),');
  var $second = $items.filter(':eq(3), :eq(4), :eq(5),');
  var $third = $items.filter(':eq(6), :eq(7), :eq(8),');
  var $fourth = $items.filter(':eq(9), :eq(10), :eq(11),');
  var $fifth = $items.filter(':eq(12), :eq(13), :eq(14),');
  var $sixith = $items.filter(':eq(15), :eq(16), :eq(17),');
  
  var arr = [$first, $second, $third, $fourth, $fifth, $sixith]; 
  
  $.each(arr, function(index, val) {
    val.wrapAll("<div/>");
  });

});
</script>

