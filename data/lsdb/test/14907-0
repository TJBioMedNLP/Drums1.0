	<script type="text/javascript">
	var jqb_vIsPause = false;
	
	$(function() {
		$('#s1').cycle({
			fx: 'fade',
			speed: 2000,
			timeout:6000,
			pager:'.jqb_navpager',
			before: onBefore
		});
	
		$('#pauseButton').click(function() {
			if (jqb_vIsPause) {
				jqb_vIsPause = false;
				$('#s1').cycle('resume');
				$("#pauseButton").removeClass("jqb_btn_play");
				$("#pauseButton").addClass("jqb_btn_pause");
			} else {
				jqb_vIsPause = true;
				$('#s1').cycle('pause');
				$("#pauseButton").removeClass("jqb_btn_pause");
				$("#pauseButton").addClass("jqb_btn_play");
			}
		});
	
		$('#s1').hover(function(){
			jqb_vIsPause = true;
			$('#s1').cycle('pause');
			$("#pauseButton").removeClass("jqb_btn_pause");
			$("#pauseButton").addClass("jqb_btn_play");
		},function(){
			jqb_vIsPause = false;
			$('#s1').cycle('resume');
			$("#pauseButton").removeClass("jqb_btn_play");
			$("#pauseButton").addClass("jqb_btn_pause");
		});
	
		$('#resumeButton').click(function() {
			$('#s1').cycle('resume');
		});
	});
	
	function onBefore() {
		var imgDesc = $(this).find('.block').html();
		$('.jqb_info').html(imgDesc);
	}
	
	</script>
	
