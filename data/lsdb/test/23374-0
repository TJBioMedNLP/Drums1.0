		function picSize () {
			if (navigator.appName=="Microsoft Internet Explorer")
			{
				myWidth=document.body.clientWidth-575;
			}
			else
			{
				myWidth=window.innerWidth-575;
			}
			if ((myWidth==Number.NaN)||(myWidth>700))
			{
				myWidth=700;
			}
			if (myWidth<500)
			{
				myWidth=500;
			}
			document.all.grouppic.innerHTML = "<img src='bilder/fotos/gruppe.jpg' alt='Gruppenfoto Dezember 2008' width='"+myWidth+"'>";
		}
		
		window.onresize = picSize;
	</script>

