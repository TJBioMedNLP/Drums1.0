    <!-- more details -->
  <br/>
  <div class="header3">Further study details as provided by Amgen:</div>
  <br/>
  <div class="indent3">

        <!-- primary outcomes -->
    <div class="body3">Primary Outcome Measures:
      <ul style="margin-top:1ex; margin-bottom:1ex;">
            <li style="margin-top:0.7ex;">Exercise tolerance, measured as peak VO(subscript)2<br/>
            </ul>
    </div>
    <br/>
    
        <!-- secondary outcomes -->
    <div class="body3">Secondary Outcome Measures:
      <ul style="margin-top:1ex; margin-bottom:1ex;">
            <li style="margin-top:0.7ex;">Exercise duration; *NYHA classification; *Patient-reported outcomes<br/>
            </ul>
    </div>
    <br/>
    
    
