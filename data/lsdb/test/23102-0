<script type="text/javascript">
// Create "about this image" link
function about_this_image_link() {
   $('body').prepend(
'<div id="about-wrapper"><div id="about_this_image_outer">\
   <div id="about_this_image_inner">\
      <a href="javascript:setCookie(\'bgStyle\',\'#dedede\',365); window.location.reload();" class="about_background_off_link">Background Off</a><span class="about_background_off_link"> | </span><a href="/" class="about_this_image_show">About</a>\
      <a href="/" class="about_this_image_hide">Restore Page</a>\
      <a href="javascript:setCookie(\'bgStyle\',\'auto\',365); window.location.reload();" id="turn_image_on_link">Background On</a>\
   </div>\
</div></div>'
   );
   $("#about_this_image_inner a.about_this_image_show").click(about_this_image_show);
   $("#about_this_image_inner a.about_this_image_hide").click(about_this_image_hide).hide();
}

// "About this image" action: hide main content, draw (toggle-able) image info box
function about_this_image_show() {
   $("#container").hide();
   $("#about_this_image_inner a.about_this_image_show").hide();
   $("#about_this_image_inner a.about_background_off_link").hide();
   $("#about_this_image_inner span.about_background_off_link").hide();
   $("#about_this_image_inner a.about_this_image_hide").show();
   $('<div id="about_this_image_info"></div><div id="ati_info_toggle">-</div>').prependTo('body');
   $('#ati_info_toggle').click(function() {
      $("#about_this_image_info").toggle(0, function() {
         if ($("#ati_info_toggle").html() == "-") {
            $("#ati_info_toggle").html("+");
         }
         else {
            $("#ati_info_toggle").html("-");
         }
      });
   });
   $('#about_this_image_info').html(
      "Students walk near Cameron Avenue as the leaves show their fall colors on the Carolina campus."
   );
   return false;
}

// Put "About this image" state back to original: show main content, destroy image info box
function about_this_image_hide() {
   $("#about_this_image_info").remove();
   $("#ati_info_toggle").remove();
   $("#container").show();
   $("#about_this_image_inner a.about_this_image_show").show();
   $("#about_this_image_inner a.about_background_off_link").show();
   $("#about_this_image_inner span.about_background_off_link").show();
   $("#about_this_image_inner a.about_this_image_hide").hide();
   return false;
}

$(document).ready(function() {
   about_this_image_link();
});
</script>

<script type="text/javascript">
function setCookie(c_name,value,expiredays)
{
	var exdate=new Date();
	exdate.setDate(exdate.getDate()+expiredays);
	document.cookie=c_name+ "=" +escape(value)+
	((expiredays==null) ? "" : ";expires="+exdate.toUTCString())+";path=/";
}

function getCookie(c_name)
{
if (document.cookie.length>0)
  {
  c_start=document.cookie.indexOf(c_name + "=");
  if (c_start!=-1)
    {
    c_start=c_start + c_name.length+1;
    c_end=document.cookie.indexOf(";",c_start);
    if (c_end==-1) c_end=document.cookie.length;
    return unescape(document.cookie.substring(c_start,c_end));
    }
  }
return "";
}

backstyle = getCookie('bgStyle');

if (backstyle!=null && backstyle!="") {
document.writeln('<style type="text/css">body { background:' + backstyle + '; }</style>');
}

if (backstyle.match("#dedede") || backstyle==null) {
document.write('<style type="text/css">.about_background_off_link, .about_this_image_hide, .about_this_image_show { display: none; }</style>');
document.write('<style type="text/css">#about_this_image_outer { width: 120px !important; background: #efefef !important; border: 1px solid #ccc; }</style>');
document.write('<style type="text/css">#about_this_image_inner { background: #efefef !important; } #about_this_image_inner a { color: #777 !important; }</style>');
} else {
document.write('<style type="text/css">#turn_image_on_link { display: none; }</style>');
}
//alert(backstyle);
</script>

<script type="text/javascript" language="javascript">
var flexiBackground = function(){
	/**
		CONFIGURATION:
		Define the size of our background image
	*/
	var bgImageSize = {
		width : 800,
		height : 600
	};
	
	/**
		Declare and define variables
	*/ 
	var $window,
		$body,
		imageID = "expando",
		tallClass = 'tall',
		wideClass = 'wide',
		$bgImage, $wrapper, img, url, imgAR;
		
	/**
		Are we dealing with ie6?
	*/
	var ie6 = ($.browser.msie && parseInt($.browser.version, 10) <= 6);
	
	/**
		Set up the action that happens on resize
	*/
	var resizeAction = function() {
		var win = {
			height : $window.height(),
			width : $window.width()
		};

		// The current aspect ratio of the window
		var winAR = win.width / win.height;

		// Determine if we need to show the image and whether it needs to stretch tall or wide
		if (win.width < bgImageSize.width && win.height < bgImageSize.height) {
			$body
				.removeClass(wideClass)
				.removeClass(tallClass);
		} else if ((win.w < bgImageSize.width && win.height >= bgImageSize.height) || (winAR < imgAR)) {
			$body
				.removeClass(wideClass)
				.addClass(tallClass);
			// Center the image
			$bgImage.css('left', Math.min(((win.width - bgImageSize.width) / 2), 0));
		} else if (win.width >= bgImageSize.width) {
			$body
				.addClass(wideClass)
				.removeClass(tallClass);
			$bgImage.css('left', 0);
		}
		
		// Need to fix the height of the wrapper for IE6
		if (ie6) {
			$wrapper.css('height', win.height);
		}
	};
	
	return {
		
		/*
			Sets up the basic functionality
		*/
		initialize : function() {
			
			// No need for any of this if the screen isn't bigger than the background image
			if (screen.availWidth <= bgImageSize.width || screen.availHeight <= bgImageSize.height) {
				return;
			}
			
			// Grab elements we'll reference throughout
			$window = $(window);
			$body = $('body');
			
			// Parse out the URL of the background image and drop out if we don't have one
			url = $body.css('background-image').replace(/^url\(("|')?|("|')?\);?$/g, '') || false;	
			if (!url || url === "none" || url === "") {
				return;
			}
			
			// Get the aspect ratio of the image
			imgAR = bgImageSize.width / bgImageSize.height;

			// Create a new image element
			$bgImage = $('<img />')
						.attr('src', url)
						.attr('id', imageID);
						
			// Create a wrapper and append the image to it.
			// The wrapper ensures we don't get scrollbars.
			$wrapper = $('<div></div>')
							.css({
								'overflow' : 'hidden',
								'width' : '100%',
								'height' : '100%',
								'z-index' : '-2'
							})
							.append($bgImage)
							.appendTo($body);
							
			// IE6 Doesn't do position: fixed, so let's fake it out.
			// We'll apply a class which gets used in the CSS to emulate position: fixed
			// Otherwise, we'll simply used position: fixed.
			if (ie6) {
				$wrapper.addClass('ie6fixed');
			} else {
				$wrapper.css({
					'position' : 'fixed',
					'top' : 0,
					'left' : 0
				});
			}
			
			// Set up a resize listener to add/remove classes from the body 
			$window.bind('resize', resizeAction);

			// Set it up by triggering a resize
			$window.trigger('resize');
		}
	};
}();

if($.browser.msie && $.browser.version=="6.0") {
   // Don't load the backdrop for IE6
} else {
$(document).ready(flexiBackground.initialize);
}
</script>
