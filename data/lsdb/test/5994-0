<script type="text/javascript">

function popup(mylink, windowname, size)
{
if(!window.focus)
return true;
var href;
if (typeof(mylink) == 'string')
href=mylink;
else
href=mylink.href;
if (size== 'small')
newwindow=window.open(href, windowname, 'width=400,height=250,scrollbar=yes');
else
newwindow=window.open(href, windowname, 'width=700,height=330,scrollbar=yes');
return false
}


