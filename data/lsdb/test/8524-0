<script>
function doSearch() {
	for(var i=0; i<4; i++) {
		if(document.searchform.search[i].checked) {
			which = document.searchform.search[i].value;
		}
	}
	var searchstring = document.searchform.searchstring.value;
	if ( searchstring == '' ) {
		alert( "Please enter a keyword." );
		document.searchform.searchstring.focus();
	}
	else
	{
		if (which == "google") {
			window.open ('http://www.google.com/search?q=' + searchstring,'','toolbar=yes,location=yes,directories=yes,status=yes,menubar=yes,scrollbars=yes,copyhistory=yes,resizable=yes');
		}
		if (which == "news") {
			window.location = "http://oardc.osu.edu/searchnews.asp?searchterms=" + searchstring;
		}
		if (which == "oardc") {
			window.location ="http://oardc.osu.edu/gresults.asp?cx=005656134412199823680:-ppcxccbqw0&cof=FORID:10&ie=UTF-8&q=" + searchstring;
		}
		if (which == "directory") {
			window.location = "http://oardc.osu.edu/phoneresults.asp?key=" + searchstring;
		}	
	}
}	
</script>
</head>
