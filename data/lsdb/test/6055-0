    function pauseResumeSlideshow(showAction) {
        var pauseButtonImage = document.getElementById('pausePlay');
        if (showAction == 'pause') {
            // User clicked back/forward button, pause slideshow
            $('#infobox-slideshow').cycle('pause');
            if (pauseButtonImage.src.search('pause') > -1) pauseButtonImage.setAttribute("src", '/assets/img/icons/control_play_blue.gif');
        } else {
            // User clicked pause/play button
            $('#infobox-slideshow').cycle('toggle');
            (pauseButtonImage.src.search('pause') > -1) ? pauseButtonImage.setAttribute("src", '/assets/img/icons/control_play_blue.gif') : pauseButtonImage.setAttribute("src", '/assets/img/icons/control_pause_blue.gif');
        }
    }
