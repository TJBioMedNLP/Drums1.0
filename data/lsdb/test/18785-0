<script type="text/javascript">
$(document).ready( function () {
	$('#buttons-row1, #buttons-row2').buttonset();

	$('#ButtonLogin').click(function() {
		//$("#overlay-top1" ).css("display","block");
		$('#overlay-top1').show('blind', {}, 500);
		$('#username').focus();
		return false;
	});

	$('#ButtonRegister').click(function() {
		//$("#overlay-top2").css("display","block");
		$('#overlay-top2').show('blind', {}, 500);
		$('#x_login').focus();
		return false;
	});

	$('#ButtonClose1, #CancelLogin').click(function() {
		//$("#overlay-top1").effect('blind', '', 'normal' );
		$('#overlay-top1').hide('blind', {}, 500);
		return false;
	});

	$('#ButtonClose2, #CancelRegister').click(function() {
		//$("#overlay-top2").effect('blind', '', 'normal' );
		$('#overlay-top2').hide('blind', {}, 500);
		return false;
	});


	$('#btnDoRegister').click(function() {
		$('#overlay-top1').hide('blind', {}, 500);
		$('#overlay-top2').show('blind', {}, 500);
		return false;
	});

});
</script>


