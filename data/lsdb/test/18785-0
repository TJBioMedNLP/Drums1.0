<script type="text/javascript">
$(function () {
    $(".ui-tag, .ui-tag-text")
		.hover(
			function(){ 
				$(this).addClass("ui-state-hover");
			},
			function(){ 
				$(this).removeClass("ui-state-hover"); 
			}
		);

	$(".ui-tag-text")
	    .click(
			function () {
				window.location = $(this).attr('caption');
		    }
		);

    $(".ui-icon-close")
		.hover(
			function(){ 
				$(this).addClass("ui-state-hover").addClass("ui-icon-circle-close"); 
			},
			function(){ 
				$(this).removeClass("ui-state-hover").removeClass("ui-icon-circle-close"); 
			}
		)
	    .click(
			function () {
				$(this).parent().hide();
				$.get("updatetags.php", { id: $(this).parent().find('.ui-tag-text').attr('caption') });
		    }
		);
});
</script>

