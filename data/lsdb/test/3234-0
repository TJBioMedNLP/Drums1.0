                  <script type="text/javascript">

		var wording = ["Every second someone downloads an article from Scitation","Scitation subscribers hail from over 195 countries across the globe","Each day, over 100,000 people visit Scitation", ];
		var gallerystartpos = 0;
		
		$(document).ready(function(){ 
			executeRotator(gallerystartpos);
		});

		function executeRotator(item) {
			item++;
			if (item > 2) item = 0;
			$("#fact").fadeIn( function() 
			{
				setTimeout( function()
				{
					$("#fact").fadeOut("slow");
					setTimeout( function()
					{
						$("#fact").html(wording[item]);
					}, 500);
					$("#fact").fadeIn("slow");
					executeRotator(item);
				}, 6000);
			});
		}

</script>

