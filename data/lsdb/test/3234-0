          <script type="text/javascript">
               $(function() { $('.boxy').boxy({modal: true}); });
               
               var newwindow;
               
               $(document).ready(function(){ 
               
                    /* AQS Listener ************ */
                    $(".aqslistener").click(function (e) {
                                                var searchcoden = '';
                        if (searchcoden == '') searchcoden = searchkey;
                        var articleInfo = $(this).parent(".authorList").attr("name");
                        AQSlistener($(this).html(),'', searchcoden, e.pageX, e.pageY, articleInfo,'/search');
                    });
                    $(".popuplink").click(function() {
                       javascript:window.open($(this).attr('href'));
                       return false;
                    });
		            $(".link_shownextprevious").hover(
		                function (e) { 
		                    var prevnextdom = $(this).attr("alt");
		                    $("#prevnext_floater").show();
		                    LaunchArticlePrevNext(prevnextdom, e.pageX, e.pageY);
		                },
		                function() {
                            $("#prevnext_floater").animate({"opacity":"0.0"},50, function() {
                                $("#prevnext_floater").hide(); 
                            });
		                }
		            );
		            $(".launchMedia").click(
		                function () { return launchMedia(this); }
		            );
		            
		            $(".launchImage").click(
		                function () { 
		                    return launchImage(this);
		                }
		            );            
		            
                    $(".articlereferenceslist li a, .edItem_block li a").click(function (e) {
                        var cvips = $(this).find("span").html();
                        if (cvips == null) return true; 
	                    var previewurl = "http://asa.aip.org./cmlink/15.470?cvips=" + cvips + "&absurl=" + escape($(this).attr("href"));
	                    LaunchAJAXContentPreview(previewurl, e.pageX, e.pageY);
	                    return false; 
                    });
                    $(".ft_firstpara .citeref_collection").remove();
                    $("#singlePreview").draggable();
                });
               
               var feedbackresolver = "http://asa.aip.org./cmlink/15.498";
               
           </script>
                        
            
                
        

          
