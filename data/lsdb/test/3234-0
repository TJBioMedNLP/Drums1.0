        <script type="text/javascript">
        //<![CDATA[
        var searchkey = "";
        var searchtype = "";
        
                
        var map;
        $(document).ready(function(){ 
        
        	var showingpanel = $(".search_content_pane:visible");
        	if(showingpanel) {
        		var showingid = showingpanel.attr('id');
        		if(showingid)
        			searchtype = showingid.substring(showingid.indexOf("_")+1, showingid.length);
        	}
        		
			if($('#searchfield').val() != "enter search here...")
				$('#searchfield').css("color","#000000");
    
        	//$("#SearchPullDown_SelectedOption img").attr("src", "searchicon-" + $('#searchzone').val() + ".png");
	        $("#SearchPullDown_SelectedOption").click(
	            function () 
	            { 
	            	$("#SearchPullDown_Wrapper").toggle('fast'); 
	            }
	        );
	        $("ul.SearchPullDown li").click(
	            function () { 
	            	updateSearchPullDown(this,'#SearchPullDown_SelectedOption'); 
	            	$("#SearchPullDown_Wrapper").toggle('fast');
	            }
	        );		
		});
       	
       	
       	
        //]]>    
        </script>
        
        
