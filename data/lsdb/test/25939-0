//<![CDATA[
function getCookieVal(offset)
{
var endstr=document.cookie.indexOf (';', offset);
if (endstr==-1) endstr=document.cookie.length;
return unescape(document.cookie.substring(offset, endstr));
}
function LireCookie(nom)
{
var arg=nom+'=';
var alen=arg.length;
var clen=document.cookie.length;
var i=0;
while (i<clen)
{
var j=i+alen;
if (document.cookie.substring(i, j)==arg) return getCookieVal(j);
i=document.cookie.indexOf(' ',i)+1;
if (i==0) break;
}
return 'z';
}
function submitbutton_fp_poll() {
var form 			= document.fp_poll16;
var radio			= form.fp_voteid;
var radioLength 	= radio.length;
var check 			= 0;
var accepteCookies 	= navigator.cookieEnabled; 
if (!accepteCookies) {
	alert('ALERT_COOKIE_ENABLED');
	return;
}
var voted			= LireCookie('d041abd1d779bd0262c24b9ea6fea908'); 
if ( voted != 'z' ) {
	alert('ALERT_ALREADY_VOTE');
	return;
}
for(var i = 0; i < radioLength; i++) {
	if (radio[i].checked) {
form.submit();
		check = 1;
	}
}	
if (check == 0) {
	alert('ALERT_NO_SELECTION');
}
}	
//]]>
