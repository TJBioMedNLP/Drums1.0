<script language="javascript">

  function protect(domain, firstName, lastName){
    var address=String.fromCharCode(116,111,58,32);
    address=String.fromCharCode(109,97,105,108)+address;
    address=address+firstName+'.'+lastName+String.fromCharCode(64)+domain;
    //alert('Email link='+address);
    window.location=address;
  }

</script>

