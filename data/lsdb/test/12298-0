<script type="text/javascript">

var sz_fachgesellschaften_slideshow = {
  init: function(index) {
    this.galleryObj = new Carousel(
        $("gallery-slider"),
        $$("#gallery-slider li"),
        $$(".gallery .link-prev", ".gallery .link-next"),
        { wheel: false, duration: 1, auto: true, frequency: 8, visibleSlides: 5, slideAmount: 5}
        );
    if (index > 0)
      this.moveToIndex(index);
    return;
  },
  moveToIndex: function(index) {
    return this.galleryObj.moveTo(this.galleryObj.slides[index]);
  }
};
sz_fachgesellschaften_slideshow.init(0);

</script>
