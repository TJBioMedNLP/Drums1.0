<script language="JavaScript" type="text/javascript">
    function WinOpen(url) {
    var param='"toolbar=no,menubar=no,scrollbars=yes,resizable=yes,width='+650+',height='+500+',screenX=100,screenY=100"';
    url='http://protein.cribi.unipd.it/'+url;
    msg=open(url,'_help',param);
    }
</script>

