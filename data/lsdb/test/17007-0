function scrivi2()
	{
		if(document.form2.wizard2.selectedIndex == 0)
		{
			document.form1.param.value = document.form1.param.value;
		}
		if(document.form2.wizard2.selectedIndex == 1)
		{
			document.form1.param.value = document.form1.param.value + 'FILTER\tC\t' + document.form2.value2_1.value;
			if(document.form2.random.checked == true)
			{
			       document.form1.param.value = document.form1.param.value + '\tRND\n';
			}
			else
			{
			       document.form1.param.value = document.form1.param.value + '\n';
			}
		}
		if(document.form2.wizard2.selectedIndex == 2)
		{
			document.form1.param.value = document.form1.param.value + 'FILTER\tA\t' + document.form2.value2_1.value;
			if(document.form2.random.checked == true)
			{
			       document.form1.param.value = document.form1.param.value + '\tRND\n';
			}
			else
			{
			       document.form1.param.value = document.form1.param.value + '\n';
			}
		}
		if(document.form2.wizard2.selectedIndex == 3)
		{
			document.form1.param.value = document.form1.param.value + 'FILTER\tT\t' + document.form2.value2_1.value ;
			if(document.form2.random.checked == true)
			{
			       document.form1.param.value = document.form1.param.value + '\tRND\n';			
			}
			else
			{
			       document.form1.param.value = document.form1.param.value + '\n';
			}
		}
		if(document.form2.wizard2.selectedIndex == 4)
		{
			document.form1.param.value = document.form1.param.value + 'FILTER\tH\t' + document.form2.value2_1.value;
			if(document.form2.random.checked == true)
			{
       				document.form1.param.value = document.form1.param.value + '\tRND\n';			
			}
			else
			{
			       document.form1.param.value = document.form1.param.value + '\n';
			}		
		}
		if(document.form2.wizard2.selectedIndex == 5)
		{
			document.form1.param.value = document.form1.param.value + 'FILTER\tS\t' + document.form2.value2_1.value;
			if(document.form2.random.checked == true)
			{
			       document.form1.param.value = document.form1.param.value + '\tRND\n';			
			}
			else
			{
			       document.form1.param.value = document.form1.param.value + '\n';
			}		
		}
		if(document.form2.wizard2.selectedIndex == 6)
		{
			document.form1.param.value = document.form1.param.value + 'FILTER\tO\t' + document.form2.value2_1.value;
			if(document.form2.random.checked == true)
			{
			       document.form1.param.value = document.form1.param.value + '\tRND\n';						
			}
			else
			{
			       document.form1.param.value = document.form1.param.value + '\n';
			}
		
		}
		if(document.form2.wizard2.selectedIndex == 7)
		{
			document.form1.param.value = document.form1.param.value + 'FILTER\tL\t' + document.form2.value2_1.value;
			if(document.form2.random.checked == true)
			{
			       document.form1.param.value = document.form1.param.value + '\tRND\n';	
			}
			else
			{
			       document.form1.param.value = document.form1.param.value + '\n';
			}		
		}
		if(document.form2.wizard2.selectedIndex == 8)
		{
			document.form1.param.value = document.form1.param.value + 'FILTER\tI\t' + document.form2.value2_1.value;
			if(document.form2.random.checked == true)
			{
			       document.form1.param.value = document.form1.param.value + '\tRND\n';						
			}
			else
			{
			       document.form1.param.value = document.form1.param.value + '\n';
			}
		}
	
	}

	function scrivi3()
	{
		if(document.form2.wizard3.selectedIndex == 0)
		{
			document.form1.param.value = document.form1.param.value + 'MAX_RESOLUTION\t' + document.form2.value3_1.value + '\n';
		}
		if(document.form2.wizard3.selectedIndex == 1)
		{
			document.form1.param.value = document.form1.param.value + 'MIN_RESOLUTION\t' + document.form2.value3_1.value + '\n';
		}
		if(document.form2.wizard3.selectedIndex == 2)
		{
			document.form1.param.value = document.form1.param.value + 'MIN_R_FREE\t' + document.form2.value3_1.value + '\n';
		}
		if(document.form2.wizard3.selectedIndex == 3)
		{
			document.form1.param.value = document.form1.param.value + 'MAX_R_FREE\t' + document.form2.value3_1.value + '\n';
		}
		if(document.form2.wizard3.selectedIndex == 4)
		{
			document.form1.param.value = document.form1.param.value + 'MIN_TAP\t' + document.form2.value3_1.value + '\n';
		}
		if(document.form2.wizard3.selectedIndex == 5)
		{
			document.form1.param.value = document.form1.param.value + 'MAX_TAP\t' + document.form2.value3_1.value + '\n';
		}
		if(document.form2.wizard3.selectedIndex == 6)
		{
			document.form1.param.value = document.form1.param.value + 'MIN_QUALITY\t' + document.form2.value3_1.value + '\n';
		}
		if(document.form2.wizard3.selectedIndex == 7)
		{
			document.form1.param.value = document.form1.param.value + 'MAX_QUALITY\t' + document.form2.value3_1.value + '\n';
		}
	}

function scrivi4()
	{
		if(document.form2.wizard4.selectedIndex == 0)
		{
			document.form1.param.value = document.form1.param.value + 'MIN_LENGTH\t' + document.form2.value4_1.value + '\n';
		}		
		if(document.form2.wizard4.selectedIndex == 1)
		{
			document.form1.param.value = document.form1.param.value + 'MAX_LENGTH\t' + document.form2.value4_1.value + '\n';
		}
		if(document.form2.wizard4.selectedIndex == 2)
		{
			document.form1.param.value = document.form1.param.value + 'NO_NMR' + '\n';
		}
		if(document.form2.wizard4.selectedIndex == 3)
		{
			document.form1.param.value = document.form1.param.value + 'NO_XRAY' + '\n';
		}
		if(document.form2.wizard4.selectedIndex == 4)
		{
			document.form1.param.value = document.form1.param.value + 'NO_SEGMENTED' + '\n';
		}
		if(document.form2.wizard4.selectedIndex == 5)
		{
			document.form1.param.value = document.form1.param.value + 'MIN_YEAR\t' + document.form2.value4_1.value + '\n';
		}
		if(document.form2.wizard4.selectedIndex == 6)
		{
			document.form1.param.value = document.form1.param.value + 'MAX_YEAR\t' + document.form2.value4_1.value + '\n';
		}
	}
