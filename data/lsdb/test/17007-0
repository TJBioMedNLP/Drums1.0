	var help1 = "";
	var help2 = "";
	var help3 = "";
	var help4 = "";
	var rand  = "";

	function prepara1()
	{
		if(document.form2.wizard1.selectedIndex == 0)
		{
			help1 = "";
			document.form2.value1_1.value = '';
		}
		if(document.form2.wizard1.selectedIndex == 1)
		{
			help1 = "INS node of CATH tree, e.g. '1' or '1.10' ... or 1.10.8.50.1.1.1.1.1";
			document.form2.value1_1.value = '1.10';
		}
		if(document.form2.wizard1.selectedIndex == 2)
		{
			help1 = "INS node of CATH tree, e.g. '1' or '1.10' ... or 1.10.8.50.1.1.1.1.1";
			document.form2.value1_1.value = '1.10';
		}
		if(document.form2.wizard1.selectedIndex == 3)
		{
			help1 = "INS minimum number of AA";
			document.form2.value1_1.value = '120';
		}
		if(document.form2.wizard1.selectedIndex == 4)
		{
			help1 = "INS maximum number of AA";
			document.form2.value1_1.value = '200';
		}				
	}


	function prepara2()
	{
		if(document.form2.wizard2.selectedIndex == 0)
		{
			help2 = "";
			document.form2.value2_1.value = '------';
			document.form2.random.disabled = true;
		}
		if(document.form2.wizard2.selectedIndex == 1)
		{
			help2 = "Find <i>n</i> proteins for each Class level";
			document.form2.value2_1.value = '1';
			document.form2.random.disabled = false;

		}
		if(document.form2.wizard2.selectedIndex == 2)
		{
			help2 = "Find <i>n</i> proteins for each Architecture level";
			document.form2.value2_1.value = '1';
			document.form2.random.disabled = false;
		}
		if(document.form2.wizard2.selectedIndex == 3)
		{
			help2 = "Find <i>n</i> proteins for each Topology level";
			document.form2.value2_1.value = '1';
			document.form2.random.disabled = false;

		}	
		if(document.form2.wizard2.selectedIndex == 4)
		{
			help2 = "Find <i>n</i> proteins for each Homology level";
			document.form2.value2_1.value = '1';
			document.form2.random.disabled = false;
		}	
		if(document.form2.wizard2.selectedIndex == 5)
		{
			help2 = "Find <i>n</i> proteins for each S level (same H and almost 35% similarity)";
			document.form2.value2_1.value = '1';
			document.form2.random.disabled = false;
		}
		if(document.form2.wizard2.selectedIndex == 6)
		{
			help2 = "Find <i>n</i> proteins for each O level (same S and almost 60% similarity)";
			document.form2.value2_1.value = '1';
			document.form2.random.disabled = false;
		}
		if(document.form2.wizard2.selectedIndex == 7)
		{
			help2 = "Find <i>n</i> proteins for each L level (same O and almost 95% similarity)";
			document.form2.value2_1.value = '1';
			document.form2.random.disabled = false;
		}
		if(document.form2.wizard2.selectedIndex == 8)
		{
			help2 = "Find <i>n</i> proteins for each I level (same L and almost 100% similarity)";
			document.form2.value2_1.value = '1';
			document.form2.random.disabled = false;
		}

	}

	function prepara3()
	{
		if(document.form2.wizard3.selectedIndex == 0)
		{
			help3 = "INS maximum resolution (999 is translated to NMR)";
			document.form2.value3_1.value = '2';
		}
		if(document.form2.wizard3.selectedIndex == 1)
		{
			help3 = "INS minimum resolution";
			document.form2.value3_1.value = '1';
		}
		if(document.form2.wizard3.selectedIndex == 2)
		{
			help3 = "INS minimum R-free factor";
			document.form2.value3_1.value = '0.2';
		}	
		if(document.form2.wizard3.selectedIndex == 3)
		{
			help3 = "INS maximum R-free factor";
			document.form2.value3_1.value = '0.3';
		}	
		if(document.form2.wizard3.selectedIndex == 4)
		{
			help3 = "INS minimum TAP [0:1]";
			document.form2.value3_1.value = '0.5';
		}
		if(document.form2.wizard3.selectedIndex == 5)
		{
			help3 = "INS maximum TAP [0:1]";
			document.form2.value3_1.value = '0.7';
		}
		if(document.form2.wizard3.selectedIndex == 6)
		{
			help3 = "INS minimum PDB-Finder quality [0:1]";
			document.form2.value3_1.value = '0.3';
		}
		if(document.form2.wizard3.selectedIndex == 7)
		{
			help3 = "INS maximum PDB-Finder quality [0:1]";
			document.form2.value3_1.value = '0.8';
		}

	}

	function prepara4()
	{
		if(document.form2.wizard4.selectedIndex == 0)
		{
			help4 = "INS minimum number of AA";
			document.form2.value4_1.value = '120';
		}
		else if(document.form2.wizard4.selectedIndex == 1)
		{
			help4 = "INS maximum number of AA";
			document.form2.value4_1.value = '200';
		}
		else if(document.form2.wizard4.selectedIndex == 5)
		{
			help4 = "INS minimum year of PDB";
			document.form2.value4_1.value = '1995';
		}
		else if(document.form2.wizard4.selectedIndex == 6)
		{
			help4 = "INS maximum year of PDB";
			document.form2.value4_1.value = '2007';
		}
		else 
		{
			help4 = "";
			document.form2.value4_1.value = '------';
		}

		
	}


	function scrivi1()
	{
		if(document.form2.wizard1.selectedIndex == 0)
		{
			alert("Please, continue by choosing more constrained conditions!");
			document.form1.param.value = document.form1.param.value + 'INCLUDE\t1\n';
			document.form1.param.value = document.form1.param.value + 'INCLUDE\t2\n';
			document.form1.param.value = document.form1.param.value + 'INCLUDE\t3\n';
			document.form1.param.value = document.form1.param.value + 'INCLUDE\t4\n';
		}		
		if(document.form2.wizard1.selectedIndex == 1)
		{
			document.form1.param.value = document.form1.param.value + 'INCLUDE\t' + document.form2.value1_1.value + '\n';
		}
		if(document.form2.wizard1.selectedIndex == 2)
		{
			document.form1.param.value = document.form1.param.value + 'EXCLUDE\t' + document.form2.value1_1.value + '\n';
		}
	}

