<script type="text/javascript">


YAHOO.namespace("fessop");


YAHOO.fessop.waiter_panel = new YAHOO.widget.Panel("waiter_panel", { width:"240px", modal:true, visible:true, fixedcenter:true, close:false, draggable:false, zindex:9999} );
YAHOO.fessop.waiter_panel.render();
YAHOO.fessop.waiter_panel.hide();



YAHOO.fessop.index_news = new YAHOO.widget.Module("index_news");
YAHOO.fessop.index_news.render();

YAHOO.fessop.faq_links = new YAHOO.widget.Module("faq_links");
YAHOO.fessop.faq_links.render();

YAHOO.fessop.index_team = new YAHOO.widget.Module("index_team");
YAHOO.fessop.index_team.render();

YAHOO.fessop.index_search = new YAHOO.widget.Module("index_search");
YAHOO.fessop.index_search.render();


var index_sendSearchButton = new YAHOO.widget.Button("query", { onclick: { fn: customSubmit } });



function customSubmit()   {
	try{
		document.body.style.overflow = 'none';
		YAHOO.fessop.waiter_panel.show();
		index_sendSearchButton.set('disabled',true);
	} catch(err) {}
	document.searchForm.submit();
}


// Takes appropriate action if IE7 or older
function detectBrowser() {
	if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)){
		var ieversion=new Number(RegExp.$1) ;
		showText("<img src='images/interface/warning.png' alt='Warning' style='float:left;margin-right:20px;'><p style='color:#ffcc00;'>Your browser (Internet Explorer "+ieversion+") may slow down with largest clusters. <a href='faq.php#whichbrowsersarecompatiblewithprofess' style='color:#ffcc00;'>More information...</a></p><br><br>", 'errorbrowser');

		if (ieversion<8) {
			document.getElementById("quickquery_div").style.margin = '-4em 0 0 1.5em';
			document.getElementById("quickquery_div").style.width = '600px';
		}
	}
}


detectBrowser();
</script>







