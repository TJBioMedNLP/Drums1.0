version=parseInt(navigator.appVersion);
valid=(navigator.appName=="Netscape" && version>2) ||
		(navigator.appName=="Microsoft Internet Explorer" && version>3);

if(valid)
{
	arrowon=new Image(6, 16);
	arrowon.src="/includes/navigators/index/images/arrowlight.gif";
	arrowoff=new Image(6, 16);
	arrowoff.src="/includes/navigators/index/images/pixelspacerlight.gif";
}

function onarrow(imgName, statText)
{
	if(valid) document[imgName].src=arrowon.src;
	window.status=statText;
	return true;
}

function offarrow(imgName)
{
	if(valid) document[imgName].src=arrowoff.src;
	window.status="";
	return true;
}
