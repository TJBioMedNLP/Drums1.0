<script>
function dl(v)
{
	//alert(v);
	document.form1.action="/lcs20/dl?p="+v+"&r="+document.getElementById("imgtext").value;
	document.form1.submit();
	//window.location.href="/lcs20/dl?p="+v+"&r="+document.getElementById("imgtext").value;
}

function newimg()
{
	var img = document.getElementById("confirmimg");
	img.src="/lcs20/ranimg?z="+Math.random();
	
}
</script>
