function TrimString(sInString) {
  sInString = sInString.replace( /^\s+/g, "" );// strip leading
  return sInString.replace( /\s+$/g, "" );// strip trailing
}

function formCheck(){
if (document.downloadtable.name.value == "") {
	alert("Please put in Name.");
	return false;
}
if (document.downloadtable.email.value == "") {
	alert("Please put in Email.");
	return false;
}
if (document.downloadtable.instititue.value == "") {
	alert("Please put in Instititue.");
	return false;
}

var xname=TrimString(document.downloadtable.name.value);
var xemail=TrimString(document.downloadtable.email.value);
var xinstititue=TrimString(document.downloadtable.instititue.value);

if (xname.length < 3) {
	alert("Please put in a valid Name.");
	return false;
}
if (xemail.length < 5) {
	alert("Please put in a valid email.");
	return false;
}
if (xinstititue.length < 2) {
	alert("Please put in a valid Instititue.");
	return false;
}

}


