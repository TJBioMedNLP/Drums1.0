	<iframe id="ctl00_RotatingBanner_usercontrols_public_billboard_ascx1_mainframe" allowTransparency="true" class="banner" frameborder="0" height="286" width="942" scrolling="no" style="background: transparent;" src="/Home/Banners/Banner12.aspx"></iframe>
	
	<!-- List of pages needs to be in reverse order (i.e. 4, 3, 2, 1), as shown 
	below -->		
	<input name="ctl00$RotatingBanner$usercontrols_public_billboard_ascx1$page1url" type="hidden" id="ctl00_RotatingBanner_usercontrols_public_billboard_ascx1_page1url" value="/Home/Banners/Banner12.aspx" />
	<input name="ctl00$RotatingBanner$usercontrols_public_billboard_ascx1$page2url" type="hidden" id="ctl00_RotatingBanner_usercontrols_public_billboard_ascx1_page2url" value="/Home/Banners/Banner10.aspx" />
	<input name="ctl00$RotatingBanner$usercontrols_public_billboard_ascx1$page3url" type="hidden" id="ctl00_RotatingBanner_usercontrols_public_billboard_ascx1_page3url" value="/Home/Banners/Banner11.aspx" />
	<input name="ctl00$RotatingBanner$usercontrols_public_billboard_ascx1$page4url" type="hidden" id="ctl00_RotatingBanner_usercontrols_public_billboard_ascx1_page4url" value="/Home/Banners/Banner8.aspx" />

	<ul class="navigation tabs">
    <li><a ID="lb_1" class="bg link1" href="?rotator=1">1</a></li>
    <li><a ID="lb_2" class="bg link2" href="?rotator=2">2</a></li>
	<li><a ID="lb_3" class="bg link3" href="?rotator=3">3</a></li>
	<li><a ID="lb_4" class="bg link4" href="?rotator=4">4</a></li>
	</ul>

</div>

<script type="text/javascript">
	$(function() {

		// This is the number of seconds (milliseconds) between transitions in the rotator.
		var rotateDelay = 5000;
		
		var rotatorData = [];

		var objRotator = $('div.rotator');

		var numItemsInSlideshow = $('input[type="hidden"]', objRotator).size();

		var currItemInSlideshow = 0;

		var iframeBanner = $('iframe.banner');



		/* Automatic tranversal through the timeline. */
		var timer = setInterval(function() { TraverseSlideshow(); }, rotateDelay);

		function TraverseSlideshow() {

			// Rewind once the end has been reached.
			currItemInSlideshow++;

			if (currItemInSlideshow >= numItemsInSlideshow)
				currItemInSlideshow = 0;

			RotateSlideshow(currItemInSlideshow);
		}

		/* Manual navigation through the timeline. */
		$('ul.navigation.tabs li a', objRotator).click(function() {

			clearTimeout(timer);

			var currIndex = $('ul.navigation.tabs li a', objRotator).index(this);

			if (currIndex == currItemInSlideshow)
				return false;

			currItemInSlideshow = currIndex;
			RotateSlideshow(currIndex);

			return false;

		});

		function RotateSlideshow(itemIdx) {

			$(iframeBanner).fadeOut(750, function() {

				$.get($('input[type="hidden"]:eq(' + itemIdx + ')', objRotator).val(), function(data, textStatus) {
					$(iframeBanner).get()[0].contentWindow.self.document.body.innerHTML = data;
					$(iframeBanner).fadeIn(750);
					$('ul.navigation.tabs li', objRotator).removeClass('current');
					$('ul.navigation.tabs li:eq(' + itemIdx + ')', objRotator).addClass('current');
				});

			});

		}

		$('ul.navigation.tabs li:first', objRotator).addClass('current');
	
	});
</script>
