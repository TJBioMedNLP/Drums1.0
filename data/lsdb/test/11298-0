</div><script type="text/javascript">
var nav4 = window.Event ? true : false;
function showKey(e,f) {
  if (nav4) // Navigator 4.0x
  {
    var whichCode = e.which
  }
  else // Internet Explorer 4.0x
  {
    if (e.type == "keypress") // the user entered a character
      var whichCode = e.keyCode
    else
      var whichCode = e.button;
  }
  if (whichCode==13)
  {
    if (f=="ls")
        ls();
    else
    {
        if (f=="google")
            doGoogle();
        else if (f=="googlefr")
            doGoogle("fr");
        else
             opensearch("");
    }
    return false;
  }
    return true;
}
function opensearch(sSubsite)
{  
  var sCountry="gb";
  var sAreas="General Information:Recreation and Wild Woods:News Releases:Publications:public register of planting and felling applications:Public Register of planting and felling applications:Learning:Forest Research";
  var sCntQual="";
  var sSubsiteUse="";
  if (sSubsite=="nationalarboreta")
    sSubsiteUse="&amp;subsite="+sSubsite;
  if (sCountry!="" & sCountry!="gb" & sCountry!="fr")
    sCntQual="+and+(field+country+contains+"+sCountry+")";
  if (sSubsite.toLowerCase()=="fr")
    window.open("/website/searchall.nsf/SearchTemplate"+sSubsite+"?OpenForm&amp;Country="+sCountry+";Areas="+sAreas+";query=%22"+document.getElementById("SearchInput").value.replace(/^\s*|\s*$/g,"")+"%22"+sCntQual+";","_self");
  else
window.open("/website/searchall.nsf/SearchTemplate?OpenForm"+sSubsiteUse+"&amp;Country="+sCountry+";Areas="+sAreas+";query=%22"+document.getElementById("SearchInput").value.replace(/^\s*|\s*$/g,"")+"%22"+sCntQual+";","_self");
//  window.open("/website/searchall.nsf/SearchTemplate"+sSubsite+"?OpenForm&c="+sCountry+"&Areas="+
//  sAreas+";query="+ document.forms[0].item("SearchInput").value+sCntQual+";","_self")
}
function ls()
{
window.open("/tools/lucene.nsf/LuceneAdv?Open&query=%2b("+document.getElementById("SearchInput").value.replace(/^\s*|\s*$/g,"")+")","_self");
}
function doGoogle(s)
{
	el=document.getElementById("SearchInput");
	if (el==null)
		return;
         	if (s=="fr")
window.open("/website/searchall.nsf/GoogleResultsFR?open=&cx=001774383426470524382:up0y5skrqss&cof=FORID:10&ie=UTF-8&q="+el.value,"_self");
	else window.open("/website/searchall.nsf/GoogleResults?open=&cx=001774383426470524382%3Aclf4ufhjvw0&cof=FORID%3A9&ie=UTF-8&q="+el.value,"_self");
	//window.open("http://www.google.com/cse?cx=001774383426470524382%3Atoxdzmmc_3u&ie=UTF-8&q="+el.value,"_self");
}
</script>
