		document.write("<TR><TD class='box-left'></TD><TD><DIV class='box-subtitle'>"+Projects[i].longTitle+"</DIV></TD><TD class='box-right'></TD></TR><TR><TD class='box-left'></TD><TD align='right'><A href='projects.htm#"+Projects[i].number+"'>");
