            <DIV class=box-title>Selected Software</DIV></TH>
          <TD class=box-right></TD></TR>
        <TR>
          <TD class=box-left></TD>
          <TD align="justify">
            <DIV class=box-extra align=left>The computer programs described herein may be freely used for non-commercial purposes. All rights to them remain the authors'.  </DIV></TD>
		</TD>
          <TD class=box-right></TD>
	</TR>

<script language="Javascript">
	var i = 0;
	for (var j = 0; j < SoftwareItemsToShowcase.length && j < Programs.length; j++) {
		i = SoftwareItemsToShowcase[j]-1;
		if (i >= Programs.length) break;
		document.write("<TR><TD class='box-left'></TD><TD><DIV class='box-subtitle'>"+Programs[i].title+"</DIV><DIV class='box-body'>"+Programs[i].description+"</DIV>");
		if (Programs[i].forDOS && !Programs[i].forUNIX) document.write("<DIV class='box-body2'>Available for Windows/DOS </DIV>");
		else if (!Programs[i].forDOS && !Programs[i].forUNIX && !Programs[i].forMAC) document.write("<DIV class='box-body2'>Available on Webserver</DIV>");
		else if (!Programs[i].forDOS && Programs[i].forUNIX) document.write("<DIV class='box-body2'>Available for UNIX/LINUX </DIV>");
		else if (!Programs[i].forDOS && !Programs[i].forUNIX) document.write("<DIV class='box-body2'>Available for MacOSX </DIV>");
		else if (Programs[i].forDOS && Programs[i].forUNIX && !Programs[i].forMAC ) document.write("<DIV class='box-body2'>Available for both Windows/DOS and UNIX/LINUX</DIV>");
		else if (Programs[i].forDOS && Programs[i].forUNIX && Programs[i].forMAC) document.write("<DIV class='box-body2'>Available for Windows/DOS, UNIX/LINUX and MacOSX</DIV>");
		
		document.write("</TD><TD class='box-right'></TD></TR><TR><TD class='box-left'></TD><TD align='right'><A href=software.htm#"+Programs[i].number+"'>");
		document.write("<TABLE class='button' cellspacing='0' cellPadding='0' width='auto' border='0'><TBODY><TR><TD class='button-left'></TD><TD class='button-fill' nowrap><A href='software.htm#"+Programs[i].number+"'>");
		document.write("<DIV class='button-title' align='center'>Details</DIV></A></TD><TD class='button-right'></TD></TR></TBODY></TABLE></A></TD><TD class='box-right'></TD></TR>");
		}
</script>

        <TR height=9>
          <TD class=box-left-divider></TD>
          <TD class=box-divider></TD>
