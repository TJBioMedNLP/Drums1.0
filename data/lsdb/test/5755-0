<script language="Javascript">
	var i = 0;
	for (var j = 0; j < PapersToShowcase.length && j < Papers.length; j++) {
		i = PapersToShowcase[j]-1;
		if (i >= Papers.length) break;
		document.write("<DIV class='box-subtitle'>"+Papers[i].title+"</DIV><DIV class='box-body'>"+Papers[i].authors+", <i>"+Papers[i].journal+"</i> ("+Papers[i].year+") ");
		if (Papers[i].volume) {
			document.write(Papers[i].volume);
			if (Papers[i].issue) document.write("("+Papers[i].issue+")");
			document.write(": ");
			}
		document.write(Papers[i].pages+"</DIV>");
		}
</script>
	</TD>
