		document.write("<TR><TD class='box-left'></TD><TD><DIV class='box-subtitle'>"+DataItems[i].title+"</DIV><DIV class='box-body'>"+DataItems[i].description+"</DIV>");
		if (DataItems[i].hasUpdate) document.write("<DIV class='box-body2'>Updated "+DataItems[i].updated+"</DIV>");
		document.write("</TD><TD class='box-right'></TD></TR><TR><TD class='box-left'></TD><TD align='right'><A href='data.htm#"+DataItems[i].number+"'>");
