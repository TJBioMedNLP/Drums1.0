function paste_example(form){
	document.PredictionForm.sequence.value=">MCA3_MOUSE\nMAAAAELRLLEKSLGLKPGNKYSAQGERQIPVLQTNNGPSLMGLSTIATHLVKQASKEHLLGSTAEEKAMVQQWLEFRVTRVDGHSSKEDTQTLLKDLNSYLEDKVYLAGHNITLADILLYYGLHRFIVDLTVQEKEKYLNVSRWFCHIQHYPDIRQHLSSIVFIKNRLYANSH\n>ACT1_ABSGL\nMSMEEEIAALVIDNGSGMCKAGFAGDDAPRAVFPSIVGRPRHQGIMVGMGQKDSYVGDEAQSKRGILTLRYPIEHGIVTNWDDMEKIWHHTFYNELRVAPEEHPVLLTEAPLNPKSNREKMTQIMFETFNAPAFYVSIQA\n";
}

function checkEmail(control) {
	if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(control.value))
	  return (true);
	else
	  return (false);
}

function validateForm(form){
	form.submit();
	return (true);
}


