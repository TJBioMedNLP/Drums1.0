<script language="JavaScript">

function format1_select() {
	document.options.user_defined_file.disabled=true;
	document.options.unigene_id.disabled=true;
	document.options.nt_list.disabled=true;	
	document.options.formatselect.value="group";
}

function enable_format3_Button() {
	document.options.format[2].checked=true;
    document.options.user_defined_file.click();
}

function format2_select() {
	document.options.unigene_id.disabled=false;
	document.options.user_defined_file.disabled=true;	
	document.options.formatselect.value="specific";	
	document.options.nt_list.disabled=true;
}

function format3_select() {
	document.options.unigene_id.disabled=true;
	document.options.user_defined_file.disabled=false;
	document.options.formatselect.value="user";
	document.options.nt_list.disabled=true;	
}
function format4_select() {
	document.options.unigene_id.disabled=true;
	document.options.user_defined_file.disabled=true;
	document.options.nt_list.disabled=false;
	document.options.formatselect.value="nt";
}
