var playlists = $H();
var current_video_url = ""; // Used to avoid reloading the video in IE

var mediaplayernum = 1; // This is incremented each time the mediaplayer div is re-written.  (Embedded object bug workaround)
var mediaplayerid = "mediaplayer" + mediaplayernum; // This is updated each time the mediaplayer div is re-written.  (Embedded object bug workaround)


function swapHighLowURL(ourName,speed)
{
	for(var c=0; c<config.lists.length; c++) {
		if (config.lists[c].listname == ourName) {
			for(var i=1; i<5 + 2; i++) {
			  if (speed == "high") {
//					alert("swapping to high: " + i + ": " + vidUrlHigh);
					config.lists[c].videos[i].url = vidUrlHigh;
				}
				else {
//					alert("swapping to low: " + i + ": " + vidUrlLow);
					config.lists[c].videos[i].url = vidUrlLow;
				}
			}
		}
	}

	// Change the external link
  if (speed == "high") {
		document.getElementById('extLink').href = vidUrlHigh;
	}
	else {
		document.getElementById('extLink').href = vidUrlLow;
	}

	// The following code is needed as a workaround for visibility issues;
	// if howtoview's CSS is initially set to display:none or visibility:hidden,
	// then Element.show('howtoview') doesn't work.
	Element.hide('howtoview');
	Element.setStyle('howtoview', {height: config.howtoview_height+'px'});

	// Automatically start playing the specified video.
	if(config.autoplay) {
		if (curPlaying != 0) {
			PlaylistClick(config.autoplay.listname, curPlaying);
		}
		else {
			PlaylistClick(config.autoplay.listname, config.autoplay.entrynumber);
		}
	}
	return;
}


function PlayerInitialize()
{
	// Create the Playlist objects based on configuration.
	for(var c=0; c<config.lists.length; c++)
		{
		playlists[config.lists[c].listname] = new Playlist(
			config.lists[c].listname,
			config.lists[c].autocollapse,
			config.lists[c].delay,
			config.lists[c].videos);
		}

	// Make sure the radio buttons are right
	

	// The following code is needed as a workaround for visibility issues;
	// if howtoview's CSS is initially set to display:none or visibility:hidden,
	// then Element.show('howtoview') doesn't work.
	Element.hide('howtoview');
	Element.setStyle('howtoview', {height: config.howtoview_height+'px'});

	// Automatically start playing the specified video.
	if(config.use_introvideo)
		{
		Play(config.introvideo);
		setTimeout("PlayerIntroStart()", 400); // This delay is to make sure that the embedded
			// object created by the Play() function has been properly initialized by the
			// browser before its "playState" value is tested.
		}
	else if(config.autoplay)
		{
		// Email a clip code
		document.getElementById('emailclip').disabled = false;
		PlaylistClick(config.autoplay.listname, config.autoplay.entrynumber);
		}
}


// PlayerIntroStart() is called, after creating the intro video object and delaying a bit,
// to set up a browser-specific routine that detects the end of the intro video.  After
// the end of the intro video, the main video will be loaded.
function PlayerIntroStart()
	{
	if(eval("document." + mediaplayerid).playState == undefined)
		{
		// This isn't IE, so use a workaround to guess when the intro video is finished.
		setTimeout("PlayerIntroDone()", config.introvideo.duration * 600);
		}
	else
		{
		// This is IE, so keep polling until the intro video is finished.
		PlayerIntroPoll();
		}
	}


function PlayerIntroPoll()
	{
	if(eval("document." + mediaplayerid).playState == 0)
		{
		PlayerIntroDone();
		}
	else if(curPlaying == 0) // Only continue polling if the user hasn't already clicked an entry.
		{
		setTimeout("PlayerIntroPoll()", 300);
		}
	}


function PlayerIntroDone()
	{
	//document.getElementById('emailclip').style.display = 'block';
	//document.getElementById('emailclip').visible = true;
	
	// Email a clip code
	document.getElementById('emailclip').disabled = false;
	//alert("done waiting");
	// Only start autoplaying if the user hasn't already clicked
	// an entry while the intro was playing.
	if(config.autoplay && (curPlaying == 0))
		{
		//alert("playing next clip");
		PlaylistClick(config.autoplay.listname, config.autoplay.entrynumber);
		}
	}



var curPlaying = 0;
var firstPlay = true;

function PlaylistToggle(listname, entrynumber) // Helper function that just calls the appropriate Playlist object's toggle() function.
	{
	return(playlists[listname].toggle(entrynumber));
	}

function PlaylistMouseover(listname, entrynumber)
	{
	return(playlists[listname].mouseover(entrynumber));
	}

function PlaylistMouseout(listname, entrynumber)
	{
	return(playlists[listname].mouseout(entrynumber));
	}

function PlaylistClick(listname, entrynumber)
	{
	curPlaying = entrynumber;
	playlists[listname].click(entrynumber);

	if(! playlists[listname].disabled)
		{
		PlaylistDisable(listname);
		Play(playlists[listname].videos[entrynumber]);
		//alert(playlists[listname].videos[entrynumber].url);
		setTimeout("PlaylistEnable('" + listname + "')", 100); // This is to prevent double clicks.

		}

    var anchor = "#clip_"+entrynumber;
    setTimeout("jumpToAnchor('" + anchor + "')",300);

	return(false);
	}

function jumpToAnchor(anchor)
    {
	window.location.href=anchor;
	}

function PlaylistDisable(listname)
	{
	playlists[listname].disabled = true;
	}

function PlaylistEnable(listname)
	{
	playlists[listname].disabled = false;
	}


// Definition of Playlist object.

function Playlist(listname, autocollapse, delay, videos)
	{
	this.running = false; // Set to true during entry-resize process.

	this.listname = listname;
	this.autocollapse = autocollapse;
	this.delay = delay; // Milliseconds to spend between fully collapsed and fully expanded.
	this.videos = videos;

	this.intervaltime = 20; // Milliseconds between updates.
	this.runningtimeleft = 0; // Time until updating stops.
	this.disabled = false; // If set to true, playlist clicks will not update the video area.

	this.entries = $H();
	this.entrynumbers = new Array();
	for(entrynumber in videos)
		{
		this.entries[entrynumber] = new PlaylistEntry(listname, entrynumber);
		this.entrynumbers.push(entrynumber);
		}
	}


Playlist.prototype.toggle = function(entrynumber)
	{
	var entry = this.entries[entrynumber];

	entry.toggle();

	if(entry.state == "opening")
		{
		if(entry.getFullHeight() > 0)
			{
			this.runningtimeleft = this.delay * (entry.getFullHeight()-entry.getCurrentHeight()) / entry.getFullHeight();
			}

		if(this.autocollapse)
			{
			//alert(this.entries[3].entrynumber);
			for(var n=0; n<this.entrynumbers.length; n++)
				{
				var entryn = this.entrynumbers[n];
				var collapse_entry = this.entries[entryn];
				//alert(collapse_entry.entrynumber);
				if(collapse_entry.entrynumber != entrynumber &&
					collapse_entry.state != "closed")
					{
					collapse_entry.startClosing();
					}
				}
			}
		}
	else if(entry.state == "closing")
		{
		if(entry.getFullHeight() > 0)
			{
			this.runningtimeleft = this.delay * Math.ceil(entry.getCurrentHeight() / entry.getFullHeight());
			}
		}


	if(! this.running)
		{
		this.running = true;
		this.jsinterval = setInterval(this.update.bind(this), this.intervaltime);
		}
	}


Playlist.prototype.mouseover = function(entrynumber)
	{
	var entry = this.entries[entrynumber];
	entry.mouseover();
	}


Playlist.prototype.mouseout = function(entrynumber)
	{
	var entry = this.entries[entrynumber];
	entry.mouseout();
	}


Playlist.prototype.click = function(entrynumber)
	{
	var entry = this.entries[entrynumber];
	if(entry.state == "closed")
		{
		this.toggle(entrynumber);
		}
	}


Playlist.prototype.update = function()
	{
	this.runningtimeleft -= this.intervaltime; // Count down timer.

	//for(entrynumber in this.entries)
	for(var n=0; n<this.entrynumbers.length; n++)
		{
		var entrynumber = this.entrynumbers[n];
		var entry = this.entries[entrynumber];

		if(entry.state == "opening")
			{
			if(this.runningtimeleft <= 0)
				{
				entry.setCurrentHeight(entry.getFullHeight());
				entry.state = "open";
				}
			else
				{
				var remainingheight = entry.getFullHeight() - entry.getCurrentHeight();

				if(remainingheight > 0)
					{
					var heightchange = remainingheight / (this.runningtimeleft/this.intervaltime);
					entry.setCurrentHeight(entry.getCurrentHeight() + heightchange);
					}
				}
			}
		else if(entry.state == "closing")
			{
			if(this.runningtimeleft <= 0)
				{
				entry.setCurrentHeight(0);
				entry.state = "closed";
				}
			else
				{
				var remainingheight = entry.getCurrentHeight();

				if(remainingheight > 0)
					{
					var heightchange = remainingheight / (this.runningtimeleft/this.intervaltime);
					entry.setCurrentHeight(entry.getCurrentHeight() - heightchange);
					}
				}
			}
		}

	if(this.runningtimeleft <= 0)
		{
		this.running = false;
		clearInterval(this.jsinterval);
		//alert("done.");
		}
	}

// Definition of PlaylistEntry object:

function PlaylistEntry(listname, entrynumber)
	{
	this.listname = listname;
	this.entrynumber = entrynumber;

	this.state = "closed";
	this.resizing = false; // Set to true when in the process of resizing.
	this.preciseheight = this.getCurrentHeight(); // Allow for fractional pixels
	}


PlaylistEntry.prototype.toggle = function()
	{
	if(this.state == "closed" || this.state == "closing")
		{
		this.startOpening();
		}
	else if(this.state == "open" || this.state == "opening")
		{
		this.startClosing();
		}
	}


PlaylistEntry.prototype.startOpening = function()
	{
	this.state = "opening";

	Element.setStyle(this.listname + '_entry_' + this.entrynumber, {backgroundColor: config.color_open});
	Element.setStyle(this.listname + '_detail_' + this.entrynumber, {backgroundColor: config.color_open});

	$(this.listname + '_button_' + this.entrynumber).src = config.button_minus;
	}


PlaylistEntry.prototype.startClosing = function()
	{
	this.state = "closing";

	Element.setStyle(this.listname + '_entry_' + this.entrynumber, {backgroundColor: config.color_closed});
	Element.setStyle(this.listname + '_detail_' + this.entrynumber, {backgroundColor: config.color_closed});

	$(this.listname + '_button_' + this.entrynumber).src = config.button_plus;
	}


PlaylistEntry.prototype.open = function()
	{
	var innerheight = Element.getHeight(this.listname + '_detailinner_' + this.entrynumber);

	Element.setStyle(this.listname + '_entry_' + this.entrynumber, {backgroundColor: config.color_open});
	Element.setStyle(this.listname + '_detail_' + this.entrynumber, {backgroundColor: config.color_open, height: innerheight+'px'});

	$(this.listname + '_button_' + this.entrynumber).src = config.button_minus;

	this.state = "open";

	return(false);
	}


PlaylistEntry.prototype.getFullHeight = function()
	{
	return(Element.getHeight(this.listname + '_detailinner_' + this.entrynumber));
	}


PlaylistEntry.prototype.getCurrentHeight = function()
	{
	var pixelheight = Element.getHeight(this.listname + '_detail_' + this.entrynumber);

	if(Math.abs(pixelheight - this.preciseheight) < 1)
		{
		return(this.preciseheight);
		}
	else
		{
		return(pixelheight);
		}
	}


PlaylistEntry.prototype.setCurrentHeight = function(height)
	{
	if(height < 0) height = 0;

	this.preciseheight = height;

	Element.setStyle(this.listname + '_detail_' + this.entrynumber, {height: Math.floor(this.preciseheight)+'px'});
	}


PlaylistEntry.prototype.mouseover = function()
	{
	if(this.state == "closed")
		{
		Element.setStyle(this.listname + '_entry_' + this.entrynumber, {backgroundColor: config.color_mouseover});
		}
	}


PlaylistEntry.prototype.mouseout = function()
	{
	if(this.state == "closed")
		{
		Element.setStyle(this.listname + '_entry_' + this.entrynumber, {backgroundColor: config.color_closed});
		}
	}


// Misc functions.

function GetStyleObject(id)
	{
	if(document.layers) return(document.layers[id]);
	else if(document.all && !document.getElementById) return(document.all[id].style);
	else if(document.getElementById && document.body.style) return(document.getElementById(id).style);

	return(false);
	}

// This function was updated for Email a Clip functionality make sure the currentPosition in the object and embed tags are set to the newoffset variable
function Play(video)
	{
	// video object must contain parameters: url, width, height, skipamount
	var newposition = 0;
	var intro = "no video";
	if(config.introvideo != undefined) {
		intro = config.introvideo.url; 
	}
	if(video.url != intro)
		CheckForLiveVideo(video.url);
	if((video.url != current_video_url) ||
		!document.all)
		{
		// Replace the entire video div if this is a new video, or if it's a non-IE browser.

		// First come up with a new id for this embedded object, as a workaround for
		// Firefox/Windows Media Player bugs.
		mediaplayernum += 1;
		mediaplayerid = "mediaplayer" + mediaplayernum;
		
		var newoffset = parseInt(video.skipamount);
		if(video.url != intro  && firstPlay == true){
			if(parseInt(0) > 0) {
				newoffset = parseInt(0);
			}
			else {
				newoffset = video.skipamount;
			}
			firstPlay = false;
		}
		
		
		$('playerdiv').innerHTML =
			'<object classid="CLSID:22D6F312-B0F6-11D0-94AB-0080C74C7E95" '+'type="application/x-oleobject" width=' + video.width + ' height=' + video.height + ' align="top" standby="Loading Windows Media Player components..." name="' + mediaplayerid + '" id="' + mediaplayerid + '"> '
			+'<param name="ShowStatusBar" value="1">'
			+'<param name="ShowDisplay" value="0">'
			+'<param name="ShowControls" value="1"> '
			+'<param name="AutoStart" value="1"> '
			+'<param name="AutoRewind" value="1">'
			+'<param name="PlayCount" value="1">'
			+'<param name="currentPosition" value="' + newoffset +'">'
			+'<param name="Appearance" value="0">'
			+'<param name="BorderStyle" value="0">'
			+'<param name="FileName" value="' + video.url + '"> '
			+'<embed name="' + mediaplayerid + '" id="' + mediaplayerid + '" src="' + video.url + '" '
			+'width="' + video.width + '" height="' + video.height + '" '
			+'autostart="1" loop="false" align="top" type="application/x-mplayer2" '
			+'pluginspage="http://www.microsoft.com/Windows/MediaPlayer/" '
			+'currentPosition="' + newoffset
			+' controls="true" showdisplay="0" showcontrols="1" showstatusbar="1" '
			+'autorewind="1" playcount="1" moviewindowheight="' + video.height + '" moviewindowwidth="' + video.width + '" '
			+'filename="' + video.url + '"> '
			+'</embed>'
			+'</object>';
		}
	else if(document.all)
		{
		// This is IE, and the link is to a new position within the same video file.
		eval("document." + mediaplayerid).CurrentPosition = video.skipamount;
		}

	current_video_url = video.url;

	return(false);
	}
	
	function CheckForLiveVideo(url)
	{
		var emailButton = document.getElementById('emailclip');
		var extension = url.substr(url.length-4, 4);
		//alert(extension);
		if(extension != '.wma' && extension != '.wmv' && extension != '.wvx') {
			emailButton.visible = false;
			emailButton.style.display = 'none';
		}
		else {
			emailButton.visible = true;
			emailButton.style.display = '';
		}
	}

function MM_preloadImages() { //v3.0
  var d=document; if(d.images){ if(!d.MM_p) d.MM_p=new Array();
    var i,j=d.MM_p.length,a=MM_preloadImages.arguments; for(i=0; i<a.length; i++)
    if (a[i].indexOf("#")!=0){ d.MM_p[j]=new Image; d.MM_p[j++].src=a[i];}}
}

function MM_swapImgRestore() { //v3.0
  var i,x,a=document.MM_sr; for(i=0;a&&i<a.length&&(x=a[i])&&x.oSrc;i++) x.src=x.oSrc;
}

function MM_findObj(n, d) { //v4.01
  var p,i,x;  if(!d) d=document; if((p=n.indexOf("?"))>0&&parent.frames.length) {
    d=parent.frames[n.substring(p+1)].document; n=n.substring(0,p);}
  if(!(x=d[n])&&d.all) x=d.all[n]; for (i=0;!x&&i<d.forms.length;i++) x=d.forms[i][n];
  for(i=0;!x&&d.layers&&i<d.layers.length;i++) x=MM_findObj(n,d.layers[i].document);
  if(!x && d.getElementById) x=d.getElementById(n); return x;
}

function MM_swapImage() { //v3.0
  var i,j=0,x,a=MM_swapImage.arguments; document.MM_sr=new Array; for(i=0;i<(a.length-2);i+=3)
   if ((x=MM_findObj(a[i]))!=null){document.MM_sr[j++]=x; if(!x.oSrc) x.oSrc=x.src; x.src=a[i+2];}
}

function MM_preloadImages() { //v3.0

  var d=document; if(d.images){ if(!d.MM_p) d.MM_p=new Array();
    var i,j=d.MM_p.length,a=MM_preloadImages.arguments; for(i=0; i<a.length; i++)
    if (a[i].indexOf("#")!=0){ d.MM_p[j]=new Image; d.MM_p[j++].src=a[i];}}
}

function MM_swapImgRestore() { //v3.0
  var i,x,a=document.MM_sr; for(i=0;a&&i<a.length&&(x=a[i])&&x.oSrc;i++) x.src=x.oSrc;
}

function MM_findObj(n, d) { //v4.01
  var p,i,x;  if(!d) d=document; if((p=n.indexOf("?"))>0&&parent.frames.length) {
    d=parent.frames[n.substring(p+1)].document; n=n.substring(0,p);}
  if(!(x=d[n])&&d.all) x=d.all[n]; for (i=0;!x&&i<d.forms.length;i++) x=d.forms[i][n];
  for(i=0;!x&&d.layers&&i<d.layers.length;i++) x=MM_findObj(n,d.layers[i].document);
  if(!x && d.getElementById) x=d.getElementById(n); return x;
}

function MM_swapImage() { //v3.0
  var i,j=0,x,a=MM_swapImage.arguments; document.MM_sr=new Array; for(i=0;i<(a.length-2);i+=3)
   if ((x=MM_findObj(a[i]))!=null){document.MM_sr[j++]=x; if(!x.oSrc) x.oSrc=x.src; x.src=a[i+2];}
}

//#####################################################
//
// Javascript for the Email a Clip functionality 
// 
// Code taken from: http://blog.paranoidferret.com/?p=4
//
//#####################################################

var sliderIntervalId = 0;
var sliderWidth = 0;
var startWidth = 415;
var sliding = false;
var slideSpeed = 10;
var emailButton;

function Slide()
{
   if(sliding)
      return;
   sliding = true;
   if(sliderWidth == startWidth)
      sliderIntervalId = setInterval('SlideUpRun()', 30);
   else {
		updateIFrame();
		sliderIntervalId = setInterval('SlideDownRun()', 30);
   }	  
}

function SlideUpRun()
{
   slider = document.getElementById('exampleSlider');
   emailButton = document.getElementById('emailclip');
   emailButton.src = "../images/email_a_clip_default.gif";
   //var scroller = document.getElementById('pscroller2');
   //scroller.style.display = '';
   if(sliderWidth <= 0)
   {
      sliding = false;
      sliderWidth = 0;
      slider.style.width = '0px';
      clearInterval(sliderIntervalId);
   }
   else
   {
      sliderWidth -= slideSpeed;
      if(sliderWidth <0)
         sliderWidth = 0;
      slider.style.width = sliderWidth + 'px';
   }
}

function SlideDownRun()
{  
   slider = document.getElementById('exampleSlider');
   emailButton = document.getElementById('emailclip');
   emailButton.src = "../images/email_a_clip_open.gif";
   //var scroller = document.getElementById('pscroller2');
   //scroller.style.display = 'none';
   if(sliderWidth>= startWidth)
   {
      sliding = false;
      sliderWidth = startWidth;
      slider.style.width = '415px';
      clearInterval(sliderIntervalId);
   }
   else
   {
      sliderWidth += slideSpeed;
      if(sliderWidth> startWidth)
         sliderWidth = startWidth;
      slider.style.width = sliderWidth + 'px';
   }
}
function updateIFrame() {
	if(curPlaying > 1) {
		var title = document.getElementById('clipTitle'+(curPlaying-1));
		var sec = document.getElementById('clipSec'+(curPlaying-1));
		var min = document.getElementById('clipMin'+(curPlaying-1));
	}
	else {
		var title = document.getElementById('clipTitle1');
		var sec = document.getElementById('clipSec1');
		var min = document.getElementById('clipMin1');
	}
	
	var player = document.getElementById(mediaplayerid);
	if(document.all)
	document.getElementById('ifEmail').src = 'emailClip.cfm?dur=' + parseInt(player.currentPosition) + '&id=4383.&player=' + curPlaying + '&max=' + parseInt(player.Duration);
	else
	document.getElementById('ifEmail').src = 'emailClip.cfm?dur=' + playlists[config.autoplay.listname].videos[curPlaying].skipamount + '&id=4383.&player=' + curPlaying + '&max=' + ((0 * 3600)+(parseInt(0)*60)+(parseInt(0)));
   
}

//#################################################
//#################################################

function EmailClip() {
	window.open('emailclip.cfm?id=#URL.id#&play=' + curPlaying + '&mark=' + playlists[config.autoplay.listname].videos[curPlaying].controls.currentPosition, 'mywin');
}
