        function openWindow(url, new_window_name) { 
    	
	   //alert (url);
	   //alert (new_window_name);
	   //alert (window.name);

           //if we are on the Gene Page do not rename the window.name!!
           if (window.name != "bottom_panel") {
              window.name = "main";
           }

           if (!(new_window_name)) {
              new_window_name = "new_window";
           }

    	   win=window.open(url,new_window_name,'width=600,height=400,menubar,toolbar,resizable,scrollbars');
        }

