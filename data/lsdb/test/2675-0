function hideShow2(ident) {
    if (document.getElementById(ident).style.display=='none') {
    	document.getElementById(ident).style.display='';
	document.images["i" + ident].src = "/CMR/images/minus.png";
    } 

   
}

function hideShow3(ident) {

   document.getElementById(ident).style.display ='none'
   document.images["i" + ident].src = "/CMR/images/plus.png";		
    
   
}

function closeTheOpenNodes(the_form){

	for(var g in open_id_array){
		
                hideShow3(open_id_array[g]);
	
	}


	
}




