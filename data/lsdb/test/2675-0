	for(var i in rank2_array){


		for(var j in store[rank2_array[i]]){
		    //alert(store[rank2_array[i]][j]);
	            if(j == 0){
		    }else{

		 	element_count++;
		    }
		    hideShow2(store[rank2_array[i]][j]);

		}
	}



}

        
