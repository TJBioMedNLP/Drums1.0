<SCRIPT LANGUAGE="JavaScript">



function OnSubmitForm()
{

  if(document.gene_search.search_type.value == 'swiss_prot')
  {
     document.gene_search.action = "shared/AccessionSearch.cgi";
  }else if(document.gene_search.search_type.value == 'genbank'){
     document.gene_search.action = "shared/AccessionSearch.cgi";
  }else if(document.gene_search.search_type.value == 'go'){
     document.gene_search.action = "shared/GoTermSearch.cgi";
  }else if(document.gene_search.search_type.value == 'hmm' ||
  document.gene_search.search_type.value == 'interpro' || document.gene_search.search_type.value == 'prosite'  || document.gene_search.search_type.value == 'cog' ){
     document.gene_search.action = "shared/EvidenceSearch.cgi";
  } else {
     document.gene_search.action ="shared/AnnotationSearch.cgi";
  }

  document.gene_search.submit();
} 
</SCRIPT> 
