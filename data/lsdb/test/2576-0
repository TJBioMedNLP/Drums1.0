<SCRIPT>
var hwndPopup_2dde;
function openpopup_2dde(url){
var popupWidth = 744;
var popupHeight = 606;
var popupTop = 300;
var popupLeft = 300;
var isFullScreen = false;
var isAutoCenter = true;
var popupTarget = "popupwin_2dde";
var popupParams = "toolbar=0, scrollbars=0, menubar=0, status=0, resizable=0";

if (isFullScreen) {
	popupParams += ", fullscreen=1";
} else if (isAutoCenter) {
	popupTop	= parseInt((window.screen.height - popupHeight)/2);
	popupLeft	= parseInt((window.screen.width - popupWidth)/2);
}

var ua = window.navigator.userAgent;
var isMac = (ua.indexOf("Mac") > -1);

//IE 5.1 PR on OSX 10.0.x does not support relative URLs in pop-ups the way they're handled below w/ document.writeln
if (isMac && url.indexOf("http") != 0) {
  url = location.href.substring(0,location.href.lastIndexOf('\/')) + "/" + url;
}

var isOpera = (ua.indexOf("Opera") > -1);
var operaVersion;
if (isOpera) {
	var i = ua.indexOf("Opera");
	operaVersion = parseFloat(ua.substring(i + 6, ua.indexOf(" ", i + 8)));
	if (operaVersion > 7.00) {
		var isAccessible = false;
		eval("try { isAccessible = ( (hwndPopup_2dde != null) && !hwndPopup_2dde.closed ); } catch(exc) { } ");
		if (!isAccessible) {
			hwndPopup_2dde = null;
		}
	}
}
if ( (hwndPopup_2dde == null) || hwndPopup_2dde.closed ) {
	
	if (isOpera && (operaVersion < 7)) {
		if (url.indexOf("http") != 0) {
			hwndPopup_2dde = window.open(url,popupTarget,popupParams + ((!isFullScreen) ? ", width=" + popupWidth +", height=" + popupHeight : ""));
			if (!isFullScreen) {
				hwndPopup_2dde.moveTo(popupLeft, popupTop);
			}
			hwndPopup_2dde.focus();
			return;
		}
	}
	if (!(window.navigator.appName == "Netscape" && !document.getElementById)) {
		//not ns4
		popupParams += ", width=" + popupWidth +", height=" + popupHeight + ", left=" + popupLeft + ", top=" + popupTop;
	} else {
		popupParams += ", left=" + popupLeft + ", top=" + popupTop;
	}
	//alert(popupParams);
	hwndPopup_2dde = window.open("",popupTarget,popupParams);
	if (!isFullScreen) {
		hwndPopup_2dde.resizeTo(popupWidth, popupHeight);
		hwndPopup_2dde.moveTo(popupLeft, popupTop);
	}
	hwndPopup_2dde.focus();
	with (hwndPopup_2dde.document) {
		open();
		write("<ht"+"ml><he"+"ad></he"+"ad><bo"+"dy onLoad=\"window.location.href='" + url + "'\"></bo"+"dy></ht"+"ml>");
		close();
	}
} else {
	if (isOpera && (operaVersion > 7.00)) {
		eval("try { hwndPopup_2dde.focus();	hwndPopup_2dde.location.href = url; } catch(exc) { hwndPopup_2dde = window.open(\""+ url +"\",\"" + popupTarget +"\",\""+ popupParams + ", width=" + popupWidth +", height=" + popupHeight +"\"); } ");
	} else {
		hwndPopup_2dde.focus();
		hwndPopup_2dde.location.href = url;
	}
}

}

</SCRIPT>
</HEAD>
