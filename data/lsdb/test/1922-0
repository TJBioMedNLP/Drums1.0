<!-- focus field -->
<script type="text/javascript" language="javascript">
function focusFieldById(target) {
	
	// set vars
	var field = document.getElementById(target);
	var fieldvalue;
	
	// if we have a field with the id 'domain'
	if(field != null) {
		
		fieldvalue = field.value;	// store current field value
		field.value = null;			// reset field
		field.focus();				// focus field
		field.value = fieldvalue;	// restore field contents
	}
}
</script>
