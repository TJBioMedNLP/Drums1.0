    function show_collections(ele) {
      Element.replace('windowShade', '<div id="windowShade" class="collections_close" onclick="hide_collections(this);">\n\
<div class="CollectionTitle_opened" width="100%">Collections</div>\n\
</div>');
      showBox(ele);

    }

    function hide_collections(ele) {
      Element.replace('windowShade', '<div id="windowShade" class="collections_open" onclick="show_collections(this);">\n\
<div class="CollectionTitle" width="100%">Collections</div>\n\
</div>');
      closeBox(ele)
    }
