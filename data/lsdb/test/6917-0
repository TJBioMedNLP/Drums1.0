<script language="JavaScript" type="text/javascript">



var selectedSubitem;
var activeMenu="subItem"+selectedSubitem;
var activeClass=document.getElementById(activeMenu);
var left = "";
var right = "";
function highlightLink(id){
	
	

	var curr =document.getElementById(id)
   

    if(curr.className != "active"){
    	curr.className = "active";
        curr.parentNode.className = "active";
    }
}
function unHighlightLink(id){

    var curr = document.getElementById(id);
    if(curr.className == "active"){
    	curr.className = "";
        curr.parentNode.className = "";
    }
}



function highlightLevel(id)
{

   
    var curr =document.getElementById(id);
    if(curr.className != "active_nav"){
    	curr.className = "active_nav";
   }
}

function unHighlightLevel2(id){
  
    var curr = document.getElementById(id);
    if(curr.className == "active_nav"){
    	curr.className = "lvl2";
    }
}
function unHighlightLevel3(id){

var activeMenu="subItem"+selectedSubitem;

if(id==activeMenu) return false;

    var curr = document.getElementById(id);
    if(curr.className == "active_nav"){
    	curr.className = "lvl3";
    }
}




</script>
