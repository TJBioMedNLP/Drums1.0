<script language="JavaScript" type="text/javascript">
<!-- PT 2247 - Function added by Sindhuja. This function is triggered to generate "Author Instructions pdf" for the journal products. The current URL is redirected with than additional query string as 'generatepdf=true'-->
function openGuideForAuthorPdf() {
	var tempUrl = location.href;
	if(location.href.indexOf("#")>-1){
		tempUrl = location.href.substring(0,location.href.indexOf("#"));
	}
    var newURL = 'http://nl.sitestat.com/elsevier/elsevier-com/s?pdf_counter_guideforauthors&ns_type=pdf&ns_url='+tempUrl + '?generatepdf=true';
	document.location.href= newURL;
}

