      var lanblock=new Array(4);
      pfeil=new Image();
      pfeil.src="pfeil1.jpg";
      leer=new Image();
      leer.src="leer1.jpg";
      lanblock[0]=new Array("de","Willkommen",new Image()); lanblock[0][2].src=pfeil.src;
      lanblock[1]=new Array("en","Welcome",new Image()); lanblock[1][2].src=leer.src;
      lanblock[2]=new Array("fr","Bienvenue",new Image()); lanblock[2][2].src=leer.src;
      lanblock[3]=new Array("ru","Добро пожаловать",new Image()); lanblock[3][2].src=leer.src;

      function aktiv(x) {
        if (x==0) document.images.pic0.src=pfeil.src; else document.images.pic0.src=leer.src;
        if (x==1) document.images.pic1.src=pfeil.src; else document.images.pic1.src=leer.src;
        if (x==2) document.images.pic2.src=pfeil.src; else document.images.pic2.src=leer.src;
        if (x==3) document.images.pic3.src=pfeil.src; else document.images.pic3.src=leer.src;

      }

    </script>
