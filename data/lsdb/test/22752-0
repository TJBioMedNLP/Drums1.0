  s_registered_user_id=null;
  }
var tmp2=getuserpf();
if (10==tmp2) s_user_group="Physician";
var tmp3=getuserct();
if ('us'==tmp3) s_user_origin="us";

function getuserinfo(){
document.writeln('<scr' + 'ipt language="JavaScript1.2" type="text/javascript">' +
"s_registered_user_id='" + s_registered_user_id + "'" +
";s_user_group='" + s_user_group + "'" +
";s_user_origin='" + s_user_origin + "'" + ';</scr' + 'ipt>');
}

getuserinfo();
</script>


