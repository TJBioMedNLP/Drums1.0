<script language="JavaScript1.2" type="text/javascript">
function getMscpUserInfo(){
  var mscpUserInfo=new Object();
  mscpUserInfo.guID=0;
  mscpUserInfo.specialtyID=0;
  mscpUserInfo.professionID=0;
  mscpUserInfo.occupationID=0;
  mscpUserInfo.country="";
  mscpUserInfo.state="";
  mscpUserInfo.campaignID=0;
  mscpUserInfo.display901AD=true;
  return mscpUserInfo;
}
</script>

