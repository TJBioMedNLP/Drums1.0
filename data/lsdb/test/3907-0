<script Language="javascript">
function clickit()
{
		if (document.theform.infoType[0].checked) {
	    	document.theform.action="info.jsp";
	    	setTimeout('df()',200);
	    }
		else if (document.theform.infoType[1].checked) {
	    	document.theform.action="domain.jsp";
	    	setTimeout('df()',200);
	    }
		else if (document.theform.infoType[2].checked) {
	    	document.theform.action="go.jsp";
	    	setTimeout('df()',200);
	    }
		else if (document.theform.infoType[3].checked) {
	    	document.theform.action="pathway.jsp";
	    	setTimeout('df()',200);
	    }
		else if (document.theform.infoType[4].checked) {
	    	document.theform.action="pdb.jsp";
	    	setTimeout('df()',200);
	    }
}
function df()
{
	document.theform.submit();
}
</script>
