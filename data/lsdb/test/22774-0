<script type="text/javascript">
    function setBigSearch(v, s){
        jQuery("#search_text_card").val(v);
        jQuery("#search_select_card").val(s);
        jQuery("#search_button_card").trigger('mouseover');
        setTimeout("doSearch()", 0);
        return false;
    }
</script>

