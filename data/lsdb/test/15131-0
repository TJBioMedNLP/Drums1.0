										reload();
                        }
                        

    //FONCTION QUI REINITIALISE LES CHAMPS DE FORMULAIRE PAR LE BOUTON CLEAR
                        
                        function clearInput(param){
                                        document.ancestor.inputSequence.value="";
                                        document.ancestor.fileSequence.value="";
                                        document.ancestor.inputArbres.value="";
                                        document.ancestor.fileArbres.value="";
                                 }
                        
    //FONCTION TESTANT L'ACTIVATION DES CHAMP D'ALIGNEMENT
                        
                        function testInputAlignment(param){
                                if(param == '1'){
                                      
                                }
                                else if(param == '2'){
                                        document.ancestor.inputArbres.disabled=false;
                                        document.ancestor.fileArbres.disabled=true;
                                        
                                        document.ancestor.inputArbres.value="";
                                        document.ancestor.fileArbres.value="";
                                }
								else if(param == '2'){
                                        document.ancestor.inputArbres.disabled=false;
                                        document.ancestor.fileArbres.disabled=true;
                                        
                                        document.ancestor.inputArbres.value="";
                                        document.ancestor.fileArbres.value="";
                                }
                                else if(param == '2'){
                                        document.ancestor.inputArbres.disabled=false;
                                        document.ancestor.fileArbres.disabled=true;
                                        
                                        document.ancestor.inputArbres.value="";
                                        document.ancestor.fileArbres.value="";
                                }
                                else{
                                        document.ancestor.inputArbres.disabled=false;
                                        document.ancestor.fileArbres.disabled=true;
                                        
                                        document.ancestor.inputArbres.value="";
                                        document.ancestor.fileArbres.value="";
                                }
                        }
                        
    //FONCTION TESTANT L'ACTIVATION DES BOUTONS RADIO DE LA PROCEDURE
                        
                        function testInputExecute(param){
                        
                                var m=document.ancestor.cochefixescenario.value;                                

                                if(param == '1'){
                                        document.ancestor.cochefixescenario.disabled=true;
                                        document.ancestor.cochepropscenario.disabled=true;
                                        document.ancestor.cochefixeposterior.disabled=true;
                                        document.ancestor.cochepropposterior.disabled=true;
                                        
                                        document.ancestor.cochefixescenario.value="";
                                        document.ancestor.cochepropscenario.value="";
                                    document.ancestor.cochefixeposterior.value="";
                                    document.ancestor.cochepropposterior.value="";
                                }
                                else if(param == '2'){
                                        document.ancestor.cochefixescenario.disabled=false;
                                        document.ancestor.cochepropscenario.disabled=false;
                                        document.ancestor.cochefixeposterior.disabled=true;
                                        document.ancestor.cochepropposterior.disabled=true;
                                        
                                        document.ancestor.cochefixescenario.value="";
                                        document.ancestor.cochepropscenario.value="";
                                    document.ancestor.cochefixeposterior.value="";
                                    document.ancestor.cochepropposterior.value="";
                                }
                                
                                else if(param == '3'){
                                        document.ancestor.cochefixescenario.disabled=true;
                                        document.ancestor.cochepropscenario.disabled=true;
                                        document.ancestor.cochefixeposterior.disabled=true;
                                        document.ancestor.cochepropposterior.disabled=true;
                                        
                                        document.ancestor.cochefixescenario.value="";
                                        document.ancestor.cochepropscenario.value="";
                                    document.ancestor.cochefixeposterior.value="";
                                    document.ancestor.cochepropposterior.value="";
                                }
                                else if(param == '4'){
                                        document.ancestor.cochefixescenario.disabled=true;
                                        document.ancestor.cochepropscenario.disabled=true;
                                        document.ancestor.cochefixeposterior.disabled=false;
                                        document.ancestor.cochepropposterior.disabled=false;
                                        
                                        document.ancestor.cochefixescenario.value="";
                                        document.ancestor.cochepropscenario.value="";
                                    document.ancestor.cochefixeposterior.value="";
                                    document.ancestor.cochepropposterior.value="";
                                }
                        }       
                        
    //FONCTION IMPOSANT DES RESTRICTIONS AUX CHAMPS DU FORMULAIRE
                        
                   function valider() {
                                        var form_err = " ";                             

                                        if ( document.ancestor.inputSequence.value.length < 1 ) {
                                                form_err = "The multiples sequence alignment field is empty.\n";
                                        }
                                                                                                                        
                                        if ( document.ancestor.inputArbres.value.length < 1 ) {
                                                form_err += "The rooted tree field is empty.\n";
                                        }
                                                                                                                        
                                        
                                        if(document.ancestor.cochefixescenario.disabled==false){
                                                
                                                var m=document.ancestor.cochefixescenario.value;
                                                var n=document.ancestor.cochepropscenario.value;
                                                
                                                if(document.ancestor.cochefixescenario.value.length >= 1 && document.ancestor.cochepropscenario.value.length >= 1 ||
                                                   document.ancestor.cochefixescenario.value.length < 1 && document.ancestor.cochepropscenario.value.length < 1)
                                                   form_err +="Please choose between a fixed or proportional scenario value.\n";
                                                else if( document.ancestor.cochefixescenario.value.length >= 1 ){
                                                                if (isNaN(m)) 
                                                                                form_err +="The fixed scenario value must be a number.\n";                      
                                                                else {
                                                                        if (m<0 )       form_err +="Your fixed scenario value must be a number m | m>=0 .\n";
                                                                }               
                                                }
                                                else if ( document.ancestor.cochepropscenario.value.length >= 1 ){
                                                                if (isNaN(n)) 
                                                                                form_err +="The proportional scenario value must be a number.\n";                       
                                                                else {
                                                                        if (n<0 || n >1 )       form_err +="Your proportional scenario value must be a number n | 0<=n<=1 .\n";
                                                                }               
                                                }

                                        }
                                        
                                
                                        if(document.ancestor.cochefixeposterior.disabled==false){
                                                
                                                var o=document.ancestor.cochefixeposterior.value;
                                                var p=document.ancestor.cochepropposterior.value;
                                                
                                                if(document.ancestor.cochefixeposterior.value.length >= 1 && document.ancestor.cochepropposterior.value.length >= 1 ||
                                                   document.ancestor.cochefixeposterior.value.length < 1 && document.ancestor.cochepropposterior.value.length < 1)
                                                   form_err +="Please choose between a fixed or proportional posterior value.\n";
                                                else if( document.ancestor.cochefixeposterior.value.length >= 1 ){
                                                                if (isNaN(o)) 
                                                                                form_err +="The fixed posterior value must be a number.\n";                     
                                                                else {
                                                                        if (o<0 )       form_err +="Your fixed posterior value must be a number o | o>=0 .\n";
                                                                }               
                                                }
                                                else if ( document.ancestor.cochepropposterior.value.length >= 1 ){
                                                                if (isNaN(p)) 
                                                                                form_err +="The proportional posterior value must be a number.\n";                      
                                                                else {
                                                                        if (p<0 || p >1 )       form_err +="Your proportional posterior value must be a number p | 0<=n<=1 .\n";
                                                                }               
                                                }

                                        }
                                        
                                        
                                        if ( document.ancestor.inputInsS.value.length < 1 ) {
                                                form_err += "InsS field empty.\n";
                                        }
                                                                                                                        
                                        if ( document.ancestor.inputInsE.value.length < 1 ) {
                                                form_err += "InsE field empty.\n";
                                        }
                                                                                
                                        if ( document.ancestor.inputdelS.value.length < 1 ) {
                                                form_err += "delS field empty.\n";
                                        }
                                        
                                        if ( document.ancestor.inputdelE.value.length < 1 ) {
                                                form_err += "delE field empty.\n";
                                        }
                                                                        
                                                var a=document.ancestor.inputInsS.value;
                                                var b=document.ancestor.inputInsE.value;
                                                var c=document.ancestor.inputdelS.value;
                                                var d=document.ancestor.inputdelE.value;

                                        if ( document.ancestor.inputInsS.value.length >= 1 ) {
                                                if (isNaN(a)) 
                                                                form_err +="The InsS value must be a number.\n";                        
                                                else {
                                                        if (a<=0 || a >1 )      form_err +="Your InsS value must be a number a | 0<a<1 .\n";
                                                         }
                                                }
                                                        
                                        if ( document.ancestor.inputInsE.value.length >= 1 ) {
                                                if (isNaN(b)) 
                                                                form_err +="The InsE value must be a number.\n";                        
                                                else {
                                                        if (b<=0 || b >1 )      form_err +="Your InsE value must be a number b | 0<b<1 .\n";
                                                        }
                                                }
                                                                                                        
                                        if ( document.ancestor.inputdelS.value.length >= 1 ) {
                                                if (isNaN(c)) 
                                                                form_err +="The delS value must be a number.\n";                        
                                                else {
                                                        if (c<=0 || c >1 )      form_err +="Your delS value must be a number c | 0<c<1 .\n";
                                                        }
                                                }
                                                        
                                        if ( document.ancestor.inputdelE.value.length >= 1 ) {
                                                if (isNaN(d)) 
                                                                form_err +="The delE value must be a number.\n";                        
                                                else {
                                                        if (d<=0 || d >1 )      form_err +="Your delE value must be a number d | 0<d<1 .\n";            
                                                        }
                                                }
        
                                        
                                        if ( form_err != " ") {
                                                alert(form_err);
                                                return false;
                                        }
                                        document.ancestor.actionAncestor.value="execute";
                                        //window.open("wait.php?id=a0c52fb46282c8d3367fb1f7cb96a102&prog=endAncestor&duree=1","_blank","width=300,height=100");
                                        x.submit();
                                        //return true
                                }
    </script>
