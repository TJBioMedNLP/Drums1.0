	//Based on Douglas Crockford	 www.crockford.com trim function
	function trim(field) {
			field.value=field.value.replace(/^\s*(\S*(\s+\S+)*)\s*$/, "$1");
	}
    
	function reload() {
		display();
		document.ancestor.actionAncestor.value="";
		document.ancestor.submit();
	}
	
	function resetFunction() {
		document.ancestor.actionAncestor.value="reset";
		reload();
		}
	
	//FONCTION TESTANT L'ACTIVATION DES CHAMPS DE FORMULAIRE SEQUENCE
	// 		
			function testUploadSequence() {				
				try
				{
				if ( document.ancestor.fileSequence.value.length < 1 ) {
					document.ancestor.inputSequence.disabled=false;
				} else {
					document.ancestor.inputSequence.disabled=true;
					reload();
				}								
				}
				catch(err)
				{		
					txt="There was an error on this page.\n\n";
					txt+="Error description: " + err.description + "\n"+err.message+"\n";
					txt+="Click OK to continue.\n\n";
					alert(txt);
				}
			}
	
	function testSequence() {				
				try
				{
				if ( document.ancestor.inputSequence.value.length > 0 ) {
					trim(document.ancestor.inputSequence);
					reload();
				}								
				}
				catch(err)
				{		
					txt="There was an error on this page.\n\n";
					txt+="Error description: " + err.description + "\n"+err.message+"\n";
					txt+="Click OK to continue.\n\n";
					alert(txt);
				}
			}
	
		function testUploadArbre() {
				try
				{
				if ( document.ancestor.fileArbres.value.length < 1 ) {
					document.ancestor.inputArbres.disabled=false;
				} else {
					document.ancestor.inputArbres.disabled=true;
					reload();
				}
				}
				catch(err)
				  {
				  txt="There was an error on this page.\n\n";
				  txt+="Error description: " + err.description + "\n"+err.message+"\n";
				  txt+="Click OK to continue.\n\n";
				  alert(txt);
				  }
			}
			
		
		function testArbre() {				
				try
				{
				if ( document.ancestor.inputArbres.value.length > 0 ) {
					trim(document.ancestor.inputArbres);
					reload();
				}
				}
				catch(err)
				{		
					txt="There was an error on this page.\n\n";
					txt+="Error description: " + err.description + "\n"+err.message+"\n";
					txt+="Click OK to continue.\n\n";
					alert(txt);
				}
			}
	                        

    //FONCTION QUI COPIE UN EXEMPLE DE SEQUENCE DANS LE CHAMPS DE FORMULAIRE SEQUENCE

                        function pasteSample1(){                                                        
                                        var x = document.ancestor;
                                        x.inputSequence.value =">D29\nGAGTGCCCAGTGGATCGGTGAGG---GTGACATGAAGCCCATCACCAAGGGCAACATGACTTC\n>L5\nGAGTGCGTCGTGGATCGGTGAAG---GCGACATGAAGCCCATCACCAAGGGCAACATGACCTC\n>Bxz2\nCTCGGCGTCGTGGACCG---GCGAGGCCGAGCGCAAGCCGATCACCAAGGGTTCGTTCGGCAA\n>Bxb1\nCTCCGCTCAGTGGATCGGTGAAGGCGACATGAAGCCCAT---CACCAAGGGCAACATGACCAA\n>TM4\nCGGTGAGTCTGCGACT-GACC--CGAAGGGCGTCAAGCCCACCAGCAAGGTGACGTGGGCCAA\n>A118\nTGGTGCTTTTTGGGTAGATGAAGCGGAACGCATTCAAAC---AAGTAAACCAACATTCACAAA\n>phi11\nAGGTGCTTACTGGGTA-GGTGAAGG--TCAAAAAATCGAAACGTCTAAGGCTACTTGGGTTAA";                                
										reload();
                        }
                        
                        
    //FONCTION QUI COPIE UN EXEMPLE D'ARBRE DANS LE CHAMPS DE FORMULAIRE ARBRE

                        function pasteSample2(){
