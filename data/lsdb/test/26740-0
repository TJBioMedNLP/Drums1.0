<script language="javascript" type="text/javascript">

   var Ly  = document.getElementById("floater");

IE = (document.all) ? 1: 0;
if (IE)
	document.all.floater.style.left=document.body.clientWidth/2+440;
else
	Ly.style.left=document.body.clientWidth/2+440;


   var fixX = Ly.style.left;
   var fixY = 300;
   var scrollDelay = 10;
   var movingSpeed = 40;
   function scrollLayer(){
   var movingDistc;
   var destY = Math.max(document.documentElement.scrollTop, document.body.scrollTop) + fixY;
   var LyTop = parseInt(Ly.style.top);
   movingDistc = ((destY - LyTop)/movingSpeed);
   Ly.style.top = LyTop + movingDistc ;
   Ly.style.left = fixX;
   setTimeout("scrollLayer()",scrollDelay);
   }
   scrollLayer();

</script>






