	function quickJumpMenuLoad() {

		if (document.getElementById('quickJumpMenu').innerHTML == "") {
			new Ajax.Request('/gf/ajax/handler.php?action=GetQuickJumpMenu',
			  {
			    method:'get',
			    onSuccess: function(transport){
