			      document.getElementById('quickJumpMenu').innerHTML = response;
			      quickJumpMenu();
			      //alert("Success! \n\n" + response);
			    },
			    onFailure: function(){ alert('ajax call failed') }
			  });	
		} else {
			quickJumpMenu();
		}
	}
