<script type="text/javascript">
 var ddlActions = $get('ctl00_SPWebPartManager1_g_f901a6f8_7b1c_49ab_82fa_9c37ad9685a8__4662daee1b27_itemListActionsControl_ddlActions');
 var submitButton = $get('ctl00_SPWebPartManager1_g_f901a6f8_7b1c_49ab_82fa_9c37ad9685a8__4662daee1b27_itemListActionsControl_btnSubmit');

    function ItemListActionsControl_DoAction()
    {
        if(ddlActions.value == 'select')
        {
            return;
        }
        switch(ddlActions.value)
        {
            case 'addToMyCollection' : 
			 window.location = "/pages/login.aspx?ReturnUrl=%2fspinejournal%2fpages%2fdefault.aspx"; 
			 break; 
case 'emailToColleague' : 
			 $find('emailToColleaguePopupExtender__1408176689').show(); 
			 break; 
case 'exportToBiblio' : 
			 $find('OpenExportToPopupExtender_166313335').show(); 
			 break; 

        }
        ddlActions.selectedIndex =0;
        submitButton.disabled = 'true';
    }
</script>

