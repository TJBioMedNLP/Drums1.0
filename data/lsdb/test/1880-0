function KWiz_SetChoiceOption(elementId)
{
    var elem=document.getElementById(elementId);
    if (elem !=null && elem !="undefined")
    elem.checked=true;
}
function KWiz_SetFocusOnControl(elementId)
{
    var elem=document.getElementById(elementId);
    if (elem !=null && elem !="undefined")
    {
        elem.focus();
        elem.select();
    }
}

