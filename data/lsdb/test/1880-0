			<tr>
				<td valign="top"><div WebPartID="ea3d2645-89f6-4a07-aedb-b7462e3bf057" HasPers="false" id="WebPartWPQ11" width="100%" class="ms-WPBody" allowDelete="false" style="display:none;" ><script language="JavaScript" type="text/javascript">

        function leftTrim(sString) {
            while (sString.substring(0, 1) == '/') {
                sString = sString.substring(1, sString.length);
            }
            return sString;
        }

        function generateHyperlink() {
            
            var myArr = new Array();
            var journalName = "";
            
            //This logic is for extracting Journal Name.
            var pathName = window.location.pathname;
            
            if (pathName.length > 0) {
                 pathName = leftTrim(pathName);
            }
            
            myArr = pathName.split("/")
            
            //Here we have got the journal name.
            if(myArr.length > 0) {
                journalName = myArr[0];
            }
            
            //To fetch host name.
            var hostName = window.location.hostname;
            
            //Static part of URL.
            var staticURL = '_layouts/1033/oaks.journals/whatsnewnursing.aspx';
            
            //Concatenating all the parts of URL.
            var mobileViewHREF = "http://" + hostName + "/" + journalName + "/" + staticURL;
            
            window.location.href = mobileViewHREF;
        }
    </script>

