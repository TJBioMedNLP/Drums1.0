<script language="javascript" type="text/javascript">

function fp_forgotPasswordValidate()
{
    var txtEmail = document.getElementById('ctl00_SPWebPartManager1_g_d6f0db30_5ad2_49b5_a627_bc0b2fdff08a_ctl00_forgotPassword_txtEmail');
    if(txtEmail.value == '')
    {
        alert('Please enter Email Address');
        txtEmail.focus();
        txtEmail.select();  
        return false;
    }
    
    var hiddenForgotPassword = document.getElementById('ctl00_SPWebPartManager1_g_d6f0db30_5ad2_49b5_a627_bc0b2fdff08a_ctl00_forgotPassword_hiddenForgotPassword');
    hiddenForgotPassword.value = 'true';
    return true;
}
function fp_closeForgotPasswordPopup()
{ 
    //hide modal popup
    var forgotPasswordPopup = $find('sendMailPanel');
    if(forgotPasswordPopup != null)
    {
        forgotPasswordPopup.hide();
    }    
        
    
    //show main panel
    var pnlForgotPassword = document.getElementById('ctl00_SPWebPartManager1_g_d6f0db30_5ad2_49b5_a627_bc0b2fdff08a_ctl00_forgotPassword_pnlMain');
    var successMessage= document.getElementById('ctl00_SPWebPartManager1_g_d6f0db30_5ad2_49b5_a627_bc0b2fdff08a_ctl00_forgotPassword_pnlPasswordSent');
    pnlForgotPassword.style.display ='block';
    successMessage.style.display = 'none'; 
    
    //hide error message
    var cvEmailError = document.getElementById('ctl00_SPWebPartManager1_g_d6f0db30_5ad2_49b5_a627_bc0b2fdff08a_ctl00_forgotPassword_cvEmailError');
    ValidatorEnable(cvEmailError, false);
    
    //clear text box
    var txtEmail = document.getElementById('ctl00_SPWebPartManager1_g_d6f0db30_5ad2_49b5_a627_bc0b2fdff08a_ctl00_forgotPassword_txtEmail');
    txtEmail.value = '';
}
function fp_showForgotPasswordPopup() {
    //hide modal popup
    var forgotPasswordPopup = $find('sendMailPanel');
    if (forgotPasswordPopup != null) {
        forgotPasswordPopup.show();
    }
} 
</script>
