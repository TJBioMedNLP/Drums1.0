<script type="text/javascript">
var atmcDescription = $get('ctl00_SPWebPartManager1_g_f901a6f8_7b1c_49ab_82fa_9c37ad9685a8_ctl00_txtDescription');
var atmcCollectionName = $get('ctl00_SPWebPartManager1_g_f901a6f8_7b1c_49ab_82fa_9c37ad9685a8_ctl00_txtCollectionName');
var atmcRdoExistingCollection = $get('ctl00_SPWebPartManager1_g_f901a6f8_7b1c_49ab_82fa_9c37ad9685a8_ctl00_rdoExistingCollection');
var atmcRdoExistingCollectionChecked = false;
var atmcRdoNewCollection = $get('ctl00_SPWebPartManager1_g_f901a6f8_7b1c_49ab_82fa_9c37ad9685a8_ctl00_rdoNewCollection');
function atmcGetPopup() { return $find('addToMyCollectionsPopupExtender_1584057857'); }
var atmcPnlMain = $get('ctl00_SPWebPartManager1_g_f901a6f8_7b1c_49ab_82fa_9c37ad9685a8_ctl00_pnlMain');
var atmcPnlMessage = $get('ctl00_SPWebPartManager1_g_f901a6f8_7b1c_49ab_82fa_9c37ad9685a8_ctl00_pnlMessage');
var atmcCmbExistingCollection = $get('ctl00_SPWebPartManager1_g_f901a6f8_7b1c_49ab_82fa_9c37ad9685a8_ctl00_cmbExistingCollection');
var atmcParaCollectionName = $get('paraCollectionName');
var atmcParaCollectionDescription = $get('paraCollectionDescription');
var atmcCustomValidator = $get('ctl00_SPWebPartManager1_g_f901a6f8_7b1c_49ab_82fa_9c37ad9685a8_ctl00_customValidator');

function atmc_attachFocusHandler() 
{
    if (atmcPnlMain != null && atmcPnlMain.style.display != 'none' && atmcGetPopup() != null)
    {
        atmcGetPopup().add_shown(atmc_SetFocusOnControl); 
    }
}

function atmc_SetFocusOnControl() {

    if (atmcCustomValidator != null && atmcCustomValidator.style.display != 'none') {
     
        atmcRdoNewCollection.checked = true;
        atmcCollectionName.disabled = false;
        atmcDescription.disabled = false;
        atmcParaCollectionName.className = "";
        atmcParaCollectionDescription.className = "";
        atmcCmbExistingCollection.disabled = true;
        atmcCollectionName.focus();
        atmcCollectionName.select();
    }
    atmcSetFocus();
}

Sys.WebForms.PageRequestManager.getInstance().add_pageLoaded(atmc_attachFocusHandler);
