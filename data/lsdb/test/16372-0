<script type="text/javascript">

function print_preview(){
	var url = self.location;
	
	if (self.location.href.indexOf('?') == -1)
	{
		url = url + '?printable=true';
	} else {
		//url = url + '&amp;printable=true';
		url = url + '&printable=true';
	}
	

	window.open(url);
	
}


</script>



	<div class="PrintPage">
		<a rel="nofollow" href="#" 
        	onclick="print_preview();return false;" target="_blank"
