<script language="javascript" type="text/javascript">
    function redirect() {
        javascript: window.location = '/search-results.aspx?term=' + document.getElementById('search').value + "&clear=1";
    }
    
    function redirectmedical() {
        javascript: window.location = '/search-results.aspx?term=' + document.getElementById('searchPanelText').value + "&clear=1";
    }

    function showhovertext(str) {
        document.getElementById('search').value = str;
    }
    function showhovermedtext(str) {
        document.getElementById('searchPanelText').value = str;
    }
    function searchBtnClick() {

        if (document.getElementById('search').value == 'Enter search term here') {
            document.getElementById('search').value = '';

        }
        setTimeout('redirect();', 10);
        return false;
    }
</script>

