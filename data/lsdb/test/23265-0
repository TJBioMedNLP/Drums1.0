<script type="text/javascript" language="javascript">
    var prm = Sys.WebForms.PageRequestManager.getInstance();
    prm.add_endRequest(EndRequestHandler);

    function EndRequestHandler(sender, args) {
        document.getElementById("shareTools").style.display = "none";
        
        $("#shareTools .input").each(function() {
                $(this).val($(this).attr('defaultValue'));
                $(this).css("color", "#ccc");
                $(this).css("font-style", "italic");

                $("#ctl00_cphMain_uxShare_uxSenderName").val("Your Name");
                $("#ctl00_cphMain_uxShare_uxToAddress").val("Recipient's Email Address");
                $("#ctl00_cphMain_uxShare_uxComments").val("Comments...");
        });
    }


    $(document).ready(function() {
        $('#shareTools .input').css("color", "#ccc");
        $('#shareTools .input').css("font-style", "italic"); 
    
        $('#shareTools .input').focus(function() {
            if ($(this).val() == $(this).attr('defaultValue')) {
                $(this).val('');
                $(this).css("color", "#000");
                $(this).css("font-style", "normal");                
            }
        });

        $('#shareTools .input').blur(function() {
            if ($(this).val() == '') {
                $(this).val($(this).attr('defaultValue'));
                $(this).css("color", "#ccc");
                $(this).css("font-style", "italic");
            }
        });


    });


    var addthis_share =
    {
    templates: {
        twitter: 'check out {{url}}'
    }
}
    function omniture(event) {
        s.linkTrackVars = "events"
        s.linkTrackEvents = "event" + event
        s.events = "event" + event
        s.tl()
    }
</script>

