<!-- Beginning of JavaScript Applet -------------------
if (navigator.preference("security.enable_java")==false) navigator.preference("security.enable_java",true);
function go(form)
{
 var destHREF=""
 if (form.links.selectedIndex==1) destHREF="";
 if (form.links.selectedIndex==2) destHREF="htmls/data.html";
 if (form.links.selectedIndex==3) destHREF="htmls/ftpinfo.html";
 if (form.links.selectedIndex==4) destHREF="htmls/jobs.html";
 if (form.links.selectedIndex==5) destHREF="htmls/updates.html";
 if (form.links.selectedIndex==6) destHREF="htmls/usage.html";
 
if (form.links.selectedIndex==9) destHREF="htmls/meeting.html";
if (form.links.selectedIndex==10) destHREF="htmls/pointers.html";

 if (form.links.selectedIndex==13) destHREF="seq/001/001.html";
 if (form.links.selectedIndex==14) destHREF="seq/002/002.html";
 if (form.links.selectedIndex==15) destHREF="seq/003/003.html";
 if (form.links.selectedIndex==16) destHREF="seq/004/004.html";
 if (form.links.selectedIndex==17) destHREF="seq/005/005.html";
 if (form.links.selectedIndex==18) destHREF="seq/006/006.html";
 if (form.links.selectedIndex==19) destHREF="seq/007/007.html";
 if (form.links.selectedIndex==20) destHREF="seq/008/008.html";



 if (destHREF != 0) 
  setTimeout('location = "' + destHREF + '"', 0);
  form.links.selectedIndex=0
}


// -- End of JavaScript code -------------- -->

