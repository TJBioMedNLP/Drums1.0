ranNum= Math.random();
if (ranNum > .1 && ranNum < .2){document.getElementById('div2').style.display="block";document.getElementById('div1').style.display="none";}
else if (ranNum > .2 && ranNum < .3){document.getElementById('div3').style.display="block";document.getElementById('div1').style.display="none";}
else if (ranNum > .3 && ranNum < .4){document.getElementById('div4').style.display="block";document.getElementById('div1').style.display="none";}
else if (ranNum > .4 && ranNum < .45){document.getElementById('div5').style.display="block";document.getElementById('div1').style.display="none";}
else if (ranNum > .45 && ranNum < .5){document.getElementById('div6').style.display="block";document.getElementById('div1').style.display="none";}
else if (ranNum > .5 && ranNum < .55){document.getElementById('div7').style.display="block";document.getElementById('div1').style.display="none";}
else if (ranNum > .55 && ranNum < .6){document.getElementById('div8').style.display="block";document.getElementById('div1').style.display="none";}
else if (ranNum > .6 && ranNum < .73){document.getElementById('div9').style.display="block";document.getElementById('div1').style.display="none";}
else if (ranNum > .73 && ranNum < .86){document.getElementById('div10').style.display="block";document.getElementById('div1').style.display="none";}
