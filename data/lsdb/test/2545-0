		function nodebrowser(thisfield, thisnodeid, thislan, browseMode, permissiontype, viewMode, selectionCount) {
			var nodelist = new String();
			var selectlist = new String();
			
			    for (var i = 0; i<document.forms[1].elements.length; i++) {
					if (document.forms[1].elements[i].name.indexOf(thisfield) > 0) {
						selectlist = document.forms[1].elements[i].name;
					}
				}
			
			for (i = 0; i < document.forms[1].elements[selectlist].options.length; i++) {
				nodelist += document.forms[1].elements[selectlist].options[i].value;
				if (i < document.forms[1].elements[selectlist].options.length - 1) {
					if(document.forms[1].elements[selectlist].options[i].value != "")
					{
						nodelist += ",";
					}
				}
			}
			path = '/public/popup.aspx?app=8&formelement=' + thisfield + '&browseMode=' + browseMode + '&checkednodes=' + escape(nodelist) + '&languageid=' + thislan + '&rootnodeid=' + thisnodeid + '&permissiontype=' + permissiontype + '&viewMode=' + viewMode + '&selectionCount=' + selectionCount;
			child = window.open(path,'','toolbar=no,width=700,height=500,directories=no,status=no,scrollbars=yes,menubar=no');
			child.creator = self;
		}
		
	
		function removeSelection(thisfield) {

			var selectlist = new String();
			
				for (var i = 0; i<document.forms[1].elements.length; i++) {
					if (document.forms[1].elements[i].name.indexOf(thisfield) > 0) {
						selectlist = document.forms[1].elements[i].name;
					}
				}

			if (document.forms[1].elements[selectlist].selectedIndex > -1) {

				document.forms[1].elements[selectlist].options[document.forms[1].elements[selectlist].selectedIndex] = null;
			
			}
			
			for (var i = 0; i<document.forms[1].elements[selectlist].length; i++) {
					document.forms[1].elements[selectlist].options[i].selected = true;
				}

		}
	
		

		function openwin(path) {
			child = window.open(path,'','toolbar=no,width=700,height=500,directories=no,status=no,scrollbars=yes,menubar=no');
			child.creator = self;
		}
		
		function closewin(){
			this.window.close();
		}
		
	    function showHide(elementid){
            if (document.getElementById(elementid).style.display == 'none'){
                document.getElementById(elementid).style.display = '';
            } else {
                document.getElementById(elementid).style.display = 'none';
        }
        
        
        
        } 

