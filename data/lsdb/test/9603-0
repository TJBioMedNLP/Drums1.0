      function highlight( item ) {
         item.style.backgroundColor = "8cccef";
      }


      function normal( item ) {
         item.style.backgroundColor = "ffffff";
      }

      function help( anchor ) {
          var helpURL = "search_help.html#" + anchor;
          helpWindow = window.open( helpURL, "helpwindow",
                                "scrollbars=yes, menubar=yes, resizable=yes, "
                                + "height=150, width=550" );
          helpWindow.focus();
      }
   </script>

