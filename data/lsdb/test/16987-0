		<script type="text/javascript">
			function doEnaTextSearch() {
				var enaTextVal = document.getElementById("text").value;
				enaTextVal = strtrim(enaTextVal);
				if (enaTextVal == "Enter search query, for example: BN000065" || enaTextVal == "")
					return;
//				location.href = "http://localhost:8080/ena/data/search?query=" + enaTextVal;
				location.href = "http://www.ebi.ac.uk/ena/data/search?query=" + enaTextVal;

			}
			
			function doEnaSequenceSearch() {
				var enaSequenceVal = strtrim(document.enaSearchPageForm.Sequence.value);
				if (enaSequenceVal == null || enaSequenceVal == "" || enaSequenceVal == "Enter or paste a nucleotide sequence")
					return;
				document.enaSearchPageForm.submit();
			}
			
			function strtrim(str) {
				return str.replace(/^\s+/,'').replace(/\s+$/,'');
			}
		</script>
