                                               event.preventDefault();
                                               self.location.href = '/DATABASE/LigandListForward?type=Peptide';
                                            }
                                         }
                                    },
                                    { name: 'Small organics', y: ligands[2], color: '#6AB296',
                                        events: {
                                             legendItemClick: function(event) {
                                               event.preventDefault();
                                               self.location.href = '/DATABASE/LigandListForward?type=Small-organic';
                                            }
                                         }
