                                       events: {
                                          click: function() {
                                              if(this.name == 'Synthetics'){
                                                    self.location.href = '/DATABASE/LigandListForward?type=Synthetic-organic';
                                              }
                                              if(this.name == 'Small organics'){
                                                    self.location.href = '/DATABASE/LigandListForward?type=Small-organic';
                                              }
                                              if(this.name == 'Natural products'){
                                                    self.location.href = '/DATABASE/LigandListForward?type=Natural-product';
                                              }
                                              if(this.name == 'Peptides'){
                                                    self.location.href = '/DATABASE/LigandListForward?type=Peptide';
                                              }
                                              if(this.name == 'Inorganics'){
                                                    self.location.href = '/DATABASE/LigandListForward?type=Inorganic';
                                              }
                                              if(this.name == 'Others'){
                                                    self.location.href = '/DATABASE/LigandListForward?type=Other';
                                              }
                                          }
                                       }
                                     }
                                  }
                             },
                             tooltip: {
                                    formatter: function() {
                                        return '<b>' + this.point.name +'</b>: '+ this.y;
                                    }
                             },
                             legend: {
                                enabled: true,
                                labelFormatter: function() {
                                    return '<b>' + this.name + '</b>: ' + this.y;
