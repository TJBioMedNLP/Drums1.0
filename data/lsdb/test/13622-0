                  <area shape="rect" coords="118,120,182,152" href="pacodom/index.php" />
                  <area shape="rect" coords="243,46,305,76" href="phinfo/index.php" />
                  <area shape="rect" coords="343,47,414,75" href="hazard/index.php" />
                  <area shape="rect" coords="295,85,355,115" href="phinet/index.php" />
                  <area shape="rect" coords="350,121,410,149" href="phinet/index.php" />
                  <area shape="rect" coords="235,121,316,150" href="pcd-base/index.php" />
