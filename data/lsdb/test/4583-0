          function displayPopup() {
           var	img = document.getElementById('txtKeywords'); 
            if (img.value =='')
            {
            alert('Please enter keyword(s) to be searched.');
            }
            else
            {
            window.location='http://nashua.case.edu/PathwaysWeb/Web/LinkForwarder.aspx?rid=KeywordSearch&rtype=br'+'&SearchTerm='+img.value;
            }
           }
