<script type="text/javascript">/*<![CDATA[*/
Deki.$(function()
{
	var uniquerefs = {};
	var refindex = 1;
	Deki.$('#pageText a.reference').each(function(i, a)
	{
		var $this = Deki.$(a);
		var href = $this.attr('href');
		if (!uniquerefs[href]) {
			uniquerefs[href] = refindex++;
		}

		$this.text('['+ uniquerefs[href] +']').click(function()
		{
			// make sure the references tab is showing
			Deki.$('#infotabswrap a.infotabs').removeClass('selected');
			Deki.$('#infotabswrap a.references').addClass('selected');
			
			Deki.$('#infotabswrap div.infoframe').hide();
			Deki.$('#referencesFrame').show();
		});
	});
});
/*]]>*/</script>			 			</div>
