		DekiWiki.$('#infotabswrap').ready(function() {
			DekiWiki.$('#infotabswrap a.infotabs').mouseover(function() {	
				DekiWiki.$('#infotabswrap a.infotabs').removeClass('selected');
				DekiWiki.$(this).addClass('selected');
				
				DekiWiki.$('#infotabswrap div.infoframe').hide();
				var infoframe = '#'+(DekiWiki.$(this).attr('rel'));
				DekiWiki.$(infoframe).show();
			});
			
			
			DekiWiki.$('#infotabswrap a.images').addClass('selected');
			var infoframe = '#'+(DekiWiki.$('#infotabswrap a.images').attr('rel'));
			DekiWiki.$(infoframe).show();
		});
		
		DekiWiki.$('#sectionEditing').ready(function() {
			DekiWiki.$('#sectionEditing').mouseover(function() {
				DekiWiki.$('#sectionEditing div.dropdown').addClass('selected');
			});
			
			DekiWiki.$('body').click(function() {	
				DekiWiki.$('#sectionEditing div.dropdown').removeClass('selected');
			});	
					
		});
		
		DekiWiki.$('#pageEditFrame').ready(function() {
			DekiWiki.$('#pageEditFrame a').mouseover(function() {
				//DekiWiki.$('#pageEditFrame a').removeClass('selected');	
				//DekiWiki.$(this).addClass('selected');
			});
			
			//DekiWiki.$('#pageEditFrame a.summary').addClass('selected');
		
		});
		
		DekiWiki.$('.topsandropdown').ready(function() {
			DekiWiki.$('.topsandropdown').mouseover(function() {
				DekiWiki.$('.topsandropdown').removeClass('selected');
				DekiWiki.$(this).addClass('selected');
			});
		
			DekiWiki.$('body').click(function() {	
				DekiWiki.$('.topsandropdown').removeClass('selected');
			});	
