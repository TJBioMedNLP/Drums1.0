<!--

var  xoonipsDocTreeView = 0;

function xoonipsGetDocTreeView(){
	var treeFrames = window.document.getElementsByName( 'treeFrame' );
	if ( treeFrames.length == 1 ){
		var treeFrame = treeFrames.item(0);
		xoonipsDocTreeView = treeFrame.contentDocument;
	}
	
	if ( xoonipsDocTreeView == undefined ){  // for IE
		xoonipsDocTreeView = window.document.treeFrame.document;
	}
}

function xoonipsOpenAll(){
    if ( xoonipsDocTreeView == 0 )
        xoonipsGetDocTreeView();
    var treeHere = xoonipsDocTreeView.getElementById('xoonipsTreeHere');
    if ( treeHere )
        treeHere.openAll();
}

function xoonipsCloseAll(){
    if ( xoonipsDocTreeView == 0 )
        xoonipsGetDocTreeView();
    var treeHere = xoonipsDocTreeView.getElementById('xoonipsTreeHere');
    if ( treeHere )
        treeHere.closeAll();
}

function xoonipsClearCheck(){
    if ( xoonipsDocTreeView == 0 )
        xoonipsGetDocTreeView();
    var treeHere = xoonipsDocTreeView.getElementById('xoonipsTreeHere');
    if ( treeHere )
        treeHere.clearCheck();
}

function xoonipsOpenTab(index_id){
    var id = 'xoonipsTab' + index_id;
    if ( xoonipsDocTreeView == 0 )
        xoonipsGetDocTreeView();
    var xoonipsTreeHere = xoonipsDocTreeView.getElementById('xoonipsTreeHere');
    var tops = new Array();
    tops[1-1] = 3;
    for ( i = 0; i < tops.length; i++ ){
        var xid = tops[i];
        var name = 'xoonipsTab' + xid;
        var la = window.document.getElementById( name +'a' );
        var lb = window.document.getElementById( name +'b' );
        var lc = window.document.getElementById( name +'c' );
        var ld = window.document.getElementById( name +'d' );
        var le = window.document.getElementById( name +'e' );
        var lf = window.document.getElementById( name +'f' );
        var l  = window.document.getElementById( name      );
        if ( index_id == xid ){
            la.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/a1.gif)";
            lb.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/b1.gif)";
            lc.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/c1.gif)";
            ld.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/d1.gif)";
            le.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/e1.gif)";
            lf.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/f1.gif)";
            l .style.zIndex=1;
            if ( xoonipsTreeHere )
                xoonipsTreeHere.selectTab( xid );
        }
        else {
            la.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/a0.gif)";
            lb.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/b0.gif)";
            lc.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/c0.gif)";
            ld.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/d0.gif)";
            le.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/e0.gif)";
            lf.style.backgroundImage = "url(http://brainatlas.brain.riken.jp/marmoset/modules/xoonips/images/f0.gif)";
            l .style.zIndex=0;
        }
    }
    return false;
}

document.xoonipsOpenTab = xoonipsOpenTab;

//-->
</script>
